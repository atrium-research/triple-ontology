        <rdfs:label>schema:spatialCoverage</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/sponsor -->


    <owl:ObjectProperty rdf:about="http://schema.org/sponsor">
        <rdfs:comment xml:lang="en">A person or organization that supports a thing through a pledge, promise, or financial contribution</rdfs:comment>
        <rdfs:label xml:lang="en">schema:sponsor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#closeMatch -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#closeMatch">
        <rdfs:comment xml:lang="en">A SKOS property indicating that two concepts are sufficiently similar to be used interchangeably in some contexts.</rdfs:comment>
        <rdfs:label xml:lang="en">skos:closeMatch</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#exactMatch -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#exactMatch">
