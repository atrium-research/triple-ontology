        <rdfs:domain rdf:resource="https://gotriple.eu/ontology/triple#Document"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Organization"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A secondary contributor to the CreativeWork</rdfs:comment>
        <rdfs:label xml:lang="en">schema:contributor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/creator -->


    <owl:ObjectProperty rdf:about="http://schema.org/creator"/>
    


    <!-- http://schema.org/funder -->


    <owl:ObjectProperty rdf:about="http://schema.org/funder">
        <rdfs:domain rdf:resource="https://gotriple.eu/ontology/triple#Document"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Organization"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A person or organization that supports something through some kind of financial contribution</rdfs:comment>
        <rdfs:label xml:lang="en">schema:funder</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/funding -->


    <owl:ObjectProperty rdf:about="http://schema.org/funding">
        <rdfs:comment xml:lang="en">A grant, typically financial or otherwise quantifiable, of resources</rdfs:comment>
        <rdfs:label xml:lang="en">schema:funding</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/inLanguage -->


    <owl:ObjectProperty rdf:about="http://schema.org/inLanguage"/>
    


    <!-- http://schema.org/keywords -->


    <owl:ObjectProperty rdf:about="http://schema.org/keywords">
