        <rdfs:comment xml:lang="en">Associates a document with its cluster (for deduplication purposes).</rdfs:comment>
        <rdfs:label xml:lang="en">in cluster</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://gotriple.eu/ontology/triple#primaryProducer -->


    <owl:ObjectProperty rdf:about="https://gotriple.eu/ontology/triple#primaryProducer">
        <rdfs:subPropertyOf rdf:resource="http://schema.org/producer"/>
        <rdfs:domain rdf:resource="https://gotriple.eu/ontology/triple#Document"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Organization"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The original source or creator of the document</rdfs:comment>
        <rdfs:label xml:lang="en">primary producer</rdfs:label>
    </owl:ObjectProperty>
    


