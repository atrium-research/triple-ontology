        <terms:language>en</terms:language>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <terms:modified>2025-10-24</terms:modified>
        <terms:publisher>GoTriple</terms:publisher>
        <terms:rights>This work is licensed under Creative Commons Attribution 4.0 International License</terms:rights>
        <terms:source rdf:resource="https://github.com/GoTriple/triple-ontology"/>
        <terms:subject>Research Data</terms:subject>
        <terms:subject>Scholarly Communication</terms:subject>
        <terms:subject>Semantic Web</terms:subject>
        <terms:subject>Social Sciences and Humanities</terms:subject>
        <terms:title xml:lang="en">TRIPLE Ontology - GoTriple Platform Data Model</terms:title>
        <vann:preferredNamespacePrefix>triple</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://gotriple.eu/ontology/triple#</vann:preferredNamespaceUri>
        <schema:citation>Bertozzi, A., De Santis, L., &amp; Peroni, S. (2025). TRIPLE Ontology - GoTriple Platform Data Model (Version 2.0.0). GoTriple. https://gotriple.eu/ontology/triple</schema:citation>
        <schema:url rdf:resource="https://gotriple.eu/ontology/triple"/>
        <schema:version>2.0.0</schema:version>
        <rdfs:comment xml:lang="en">Comprehensive semantic representation of the GoTriple discovery platform&apos;s data model for Social Sciences and Humanities (SSH) research artifacts.</rdfs:comment>
        <rdfs:label xml:lang="en">TRIPLE Ontology</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://gotriple.eu/"/>
        <rdfs:seeAlso rdf:resource="https://www.samod.org/"/>
        <owl:priorVersion rdf:resource="https://gotriple.eu/ontology/triple/1.0.0"/>
        <owl:versionInfo>2.0.0</owl:versionInfo>
        <dcat:keyword>humanities</dcat:keyword>
        <dcat:keyword>ontology</dcat:keyword>
        <dcat:keyword>research data</dcat:keyword>
        <dcat:keyword>semantic web</dcat:keyword>
        <dcat:keyword>social sciences</dcat:keyword>
    </owl:Ontology>
    


