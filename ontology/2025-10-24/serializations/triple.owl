        <rdfs:comment xml:lang="en">A SKOS property linking a concept to the concept scheme it belongs to.</rdfs:comment>
        <rdfs:label xml:lang="en">skos:inScheme</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://xmlns.com/foaf/0.1/account -->


    <owl:ObjectProperty rdf:about="http://xmlns.com/foaf/0.1/account">
        <rdfs:comment xml:lang="en">Indicates an account held by this agent</rdfs:comment>
        <rdfs:label xml:lang="en">foaf:account</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://gotriple.eu/ontology/triple#aggregator -->


    <owl:ObjectProperty rdf:about="https://gotriple.eu/ontology/triple#aggregator">
        <rdfs:subPropertyOf rdf:resource="http://schema.org/contributor"/>
        <rdfs:domain rdf:resource="https://gotriple.eu/ontology/triple#Document"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Organization"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">An aggregator that collates documents from different sources</rdfs:comment>
        <rdfs:label xml:lang="en">aggregator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://gotriple.eu/ontology/triple#alsoKnownAs -->


    <owl:ObjectProperty rdf:about="https://gotriple.eu/ontology/triple#alsoKnownAs">
        <rdfs:domain rdf:resource="https://gotriple.eu/ontology/triple#Profile"/>
        <rdfs:range rdf:resource="https://gotriple.eu/ontology/triple#Profile"/>
        <rdfs:comment xml:lang="en">Links an ambiguous profile to the original profile</rdfs:comment>
        <rdfs:label xml:lang="en">also known as</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://gotriple.eu/ontology/triple#inCluster -->


    <owl:ObjectProperty rdf:about="https://gotriple.eu/ontology/triple#inCluster">
