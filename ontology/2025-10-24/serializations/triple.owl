    <!-- http://purl.org/dc/elements/1.1/subject -->


    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/subject">
        <rdfs:comment xml:lang="en">The subject or topic of the document, using a term from a controlled vocabulary.</rdfs:comment>
        <rdfs:label xml:lang="en">dc:subject</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/elements/1.1/type -->


    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/type">
        <rdfs:comment xml:lang="en">The type of the document (e.g., Article, Dataset, Book).</rdfs:comment>
        <rdfs:label xml:lang="en">dc:type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/datacite/hasIdentifier -->


    <owl:ObjectProperty rdf:about="http://purl.org/spar/datacite/hasIdentifier">
        <rdfs:label xml:lang="en">datacite:hasIdentifier</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/datacite/usesIdentifierScheme -->


    <owl:ObjectProperty rdf:about="http://purl.org/spar/datacite/usesIdentifierScheme">
        <rdfs:label xml:lang="en">datacite:usesIdentifierScheme</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/about -->


    <owl:ObjectProperty rdf:about="http://schema.org/about">
        <rdfs:comment xml:lang="en">The subject matter of the content</rdfs:comment>
        <rdfs:label xml:lang="en">schema:about</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/author -->


    <owl:ObjectProperty rdf:about="http://schema.org/author">
        <rdfs:subPropertyOf rdf:resource="http://schema.org/creator"/>
        <rdfs:domain rdf:resource="https://gotriple.eu/ontology/triple#Document"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Organization"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The author of this content</rdfs:comment>
        <rdfs:label xml:lang="en">schema:author</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/conditionsOfAccess -->


    <owl:ObjectProperty rdf:about="http://schema.org/conditionsOfAccess">
        <rdfs:comment xml:lang="en">The conditions under which the document can be accessed (e.g., Open Access, Restricted).</rdfs:comment>
        <rdfs:label xml:lang="en">schema:conditionsOfAccess</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/contributor -->


