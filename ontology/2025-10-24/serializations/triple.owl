                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The service provider, organization, or individual that provides the data</rdfs:comment>
        <rdfs:label xml:lang="en">schema:provider</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/publisher -->


    <owl:ObjectProperty rdf:about="http://schema.org/publisher">
        <rdfs:domain rdf:resource="https://gotriple.eu/ontology/triple#Document"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Organization"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The publisher of the creative work</rdfs:comment>
