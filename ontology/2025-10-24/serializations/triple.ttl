<http://schema.org/version> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#maxCardinality
owl:maxCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
<http://www.w3.org/2004/02/skos/core#definition> rdf:type owl:AnnotationProperty ;
                                                 rdfs:comment "A SKOS annotation property providing a complete explanation of the intended meaning of a concept."@en ;
                                                 rdfs:label "skos:definition"@en .


###  http://www.w3.org/ns/dcat#keyword
<http://www.w3.org/ns/dcat#keyword> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://schema.org/DateTime
<http://schema.org/DateTime> rdf:type rdfs:Datatype .


###  http://schema.org/Text
<http://schema.org/Text> rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/elements/1.1/subject
<http://purl.org/dc/elements/1.1/subject> rdf:type owl:ObjectProperty ;
                                          rdfs:comment "The subject or topic of the document, using a term from a controlled vocabulary."@en ;
                                          rdfs:label "dc:subject"@en .


###  http://purl.org/dc/elements/1.1/type
<http://purl.org/dc/elements/1.1/type> rdf:type owl:ObjectProperty ;
                                       rdfs:comment "The type of the document (e.g., Article, Dataset, Book)."@en ;
                                       rdfs:label "dc:type"@en .


###  http://purl.org/spar/datacite/hasIdentifier
<http://purl.org/spar/datacite/hasIdentifier> rdf:type owl:ObjectProperty ;
                                              rdfs:label "datacite:hasIdentifier"@en .


###  http://purl.org/spar/datacite/usesIdentifierScheme
<http://purl.org/spar/datacite/usesIdentifierScheme> rdf:type owl:ObjectProperty ;
                                                     rdfs:label "datacite:usesIdentifierScheme"@en .


###  http://schema.org/about
<http://schema.org/about> rdf:type owl:ObjectProperty ;
                          rdfs:comment "The subject matter of the content"@en ;
                          rdfs:label "schema:about"@en .


###  http://schema.org/author
<http://schema.org/author> rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf <http://schema.org/creator> ;
                           rdfs:domain :Document ;
                           rdfs:range [ rdf:type owl:Class ;
                                        owl:unionOf ( <http://xmlns.com/foaf/0.1/Organization>
                                                      <http://xmlns.com/foaf/0.1/Person>
                                                    )
                                      ] ;
                           rdfs:comment "The author of this content"@en ;
                           rdfs:label "schema:author"@en .


###  http://schema.org/conditionsOfAccess
<http://schema.org/conditionsOfAccess> rdf:type owl:ObjectProperty ;
                                       rdfs:comment "The conditions under which the document can be accessed (e.g., Open Access, Restricted)."@en ;
                                       rdfs:label "schema:conditionsOfAccess"@en .


###  http://schema.org/contributor
<http://schema.org/contributor> rdf:type owl:ObjectProperty ;
                                rdfs:domain :Document ;
                                rdfs:range [ rdf:type owl:Class ;
                                             owl:unionOf ( <http://xmlns.com/foaf/0.1/Organization>
                                                           <http://xmlns.com/foaf/0.1/Person>
                                                         )
                                           ] ;
                                rdfs:comment "A secondary contributor to the CreativeWork"@en ;
                                rdfs:label "schema:contributor"@en .


###  http://schema.org/creator
<http://schema.org/creator> rdf:type owl:ObjectProperty .


###  http://schema.org/funder
<http://schema.org/funder> rdf:type owl:ObjectProperty ;
                           rdfs:domain :Document ;
                           rdfs:range [ rdf:type owl:Class ;
                                        owl:unionOf ( <http://xmlns.com/foaf/0.1/Organization>
                                                      <http://xmlns.com/foaf/0.1/Person>
                                                    )
                                      ] ;
                           rdfs:comment "A person or organization that supports something through some kind of financial contribution"@en ;
                           rdfs:label "schema:funder"@en .


###  http://schema.org/funding
<http://schema.org/funding> rdf:type owl:ObjectProperty ;
                            rdfs:comment "A grant, typically financial or otherwise quantifiable, of resources"@en ;
                            rdfs:label "schema:funding"@en .


###  http://schema.org/inLanguage
<http://schema.org/inLanguage> rdf:type owl:ObjectProperty .


###  http://schema.org/keywords
<http://schema.org/keywords> rdf:type owl:ObjectProperty ;
                             rdfs:comment "Keywords or tags used to describe this content"@en ;
                             rdfs:label "schema:keywords"@en .


###  http://schema.org/license
<http://schema.org/license> rdf:type owl:ObjectProperty ;
                            rdfs:comment "The license under which the document is published (e.g., CC-BY, CC0)."@en ;
                            rdfs:label "schema:license"@en .


###  http://schema.org/mentions
<http://schema.org/mentions> rdf:type owl:ObjectProperty .


###  http://schema.org/producer
<http://schema.org/producer> rdf:type owl:ObjectProperty ;
                             rdfs:domain :Document ;
                             rdfs:range [ rdf:type owl:Class ;
                                          owl:unionOf ( <http://xmlns.com/foaf/0.1/Organization>
                                                        <http://xmlns.com/foaf/0.1/Person>
                                                      )
                                        ] ;
                             rdfs:comment "The person or organization who produced the work"@en ;
                             rdfs:label "schema:producer"@en .


###  http://schema.org/provider
<http://schema.org/provider> rdf:type owl:ObjectProperty ;
                             rdfs:domain :Document ;
                             rdfs:range [ rdf:type owl:Class ;
                                          owl:unionOf ( <http://xmlns.com/foaf/0.1/Organization>
                                                        <http://xmlns.com/foaf/0.1/Person>
                                                      )
                                        ] ;
                             rdfs:comment "The service provider, organization, or individual that provides the data"@en ;
                             rdfs:label "schema:provider"@en .


###  http://schema.org/publisher
<http://schema.org/publisher> rdf:type owl:ObjectProperty ;
                              rdfs:domain :Document ;
                              rdfs:range [ rdf:type owl:Class ;
                                           owl:unionOf ( <http://xmlns.com/foaf/0.1/Organization>
                                                         <http://xmlns.com/foaf/0.1/Person>
                                                       )
                                         ] ;
                              rdfs:comment "The publisher of the creative work"@en ;
                              rdfs:label "schema:publisher"@en .


###  http://schema.org/spatialCoverage
<http://schema.org/spatialCoverage> rdf:type owl:ObjectProperty ;
                                    rdfs:domain :Document ;
                                    rdfs:range <http://schema.org/Place> ;
                                    rdfs:label "schema:spatialCoverage" .


###  http://schema.org/sponsor
<http://schema.org/sponsor> rdf:type owl:ObjectProperty ;
                            rdfs:comment "A person or organization that supports a thing through a pledge, promise, or financial contribution"@en ;
                            rdfs:label "schema:sponsor"@en .


###  http://www.w3.org/2004/02/skos/core#closeMatch
<http://www.w3.org/2004/02/skos/core#closeMatch> rdf:type owl:ObjectProperty ;
                                                 rdfs:comment "A SKOS property indicating that two concepts are sufficiently similar to be used interchangeably in some contexts."@en ;
                                                 rdfs:label "skos:closeMatch"@en .


###  http://www.w3.org/2004/02/skos/core#exactMatch
<http://www.w3.org/2004/02/skos/core#exactMatch> rdf:type owl:ObjectProperty ;
                                                 rdfs:comment "A SKOS property indicating that two concepts can be used interchangeably across all contexts."@en ;
                                                 rdfs:label "skos:exactMatch"@en .


###  http://www.w3.org/2004/02/skos/core#inScheme
<http://www.w3.org/2004/02/skos/core#inScheme> rdf:type owl:ObjectProperty ;
                                               rdfs:comment "A SKOS property linking a concept to the concept scheme it belongs to."@en ;
                                               rdfs:label "skos:inScheme"@en .


###  http://xmlns.com/foaf/0.1/account
<http://xmlns.com/foaf/0.1/account> rdf:type owl:ObjectProperty ;
                                    rdfs:comment "Indicates an account held by this agent"@en ;
                                    rdfs:label "foaf:account"@en .


###  https://gotriple.eu/ontology/triple#aggregator
:aggregator rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://schema.org/contributor> ;
            rdfs:domain :Document ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( <http://xmlns.com/foaf/0.1/Organization>
                                       <http://xmlns.com/foaf/0.1/Person>
                                     )
                       ] ;
            rdfs:comment "An aggregator that collates documents from different sources"@en ;
            rdfs:label "aggregator"@en .


###  https://gotriple.eu/ontology/triple#alsoKnownAs
:alsoKnownAs rdf:type owl:ObjectProperty ;
             rdfs:domain :Profile ;
             rdfs:range :Profile ;
             rdfs:comment "Links an ambiguous profile to the original profile"@en ;
             rdfs:label "also known as"@en .


###  https://gotriple.eu/ontology/triple#inCluster
:inCluster rdf:type owl:ObjectProperty ;
           rdfs:domain :Document ;
           rdfs:range :Cluster ;
           rdfs:comment "Associates a document with its cluster (for deduplication purposes)."@en ;
           rdfs:label "in cluster"@en .


###  https://gotriple.eu/ontology/triple#primaryProducer
:primaryProducer rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://schema.org/producer> ;
                 rdfs:domain :Document ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( <http://xmlns.com/foaf/0.1/Organization>
                                            <http://xmlns.com/foaf/0.1/Person>
                                          )
                            ] ;
                 rdfs:comment "The original source or creator of the document"@en ;
                 rdfs:label "primary producer"@en .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/spar/literal/hasLiteralValue
<http://purl.org/spar/literal/hasLiteralValue> rdf:type owl:DatatypeProperty .


###  http://schema.org/abstract
<http://schema.org/abstract> rdf:type owl:DatatypeProperty .


###  http://schema.org/alternateName
<http://schema.org/alternateName> rdf:type owl:DatatypeProperty ;
                                  rdfs:range rdf:langString ;
                                  rdfs:comment "An alias for the item"@en ;
                                  rdfs:label "schema:alternateName"@en .


###  http://schema.org/datePublished
<http://schema.org/datePublished> rdf:type owl:DatatypeProperty ;
                                  rdfs:domain :Document ;
                                  rdfs:range xsd:date ;
                                  rdfs:comment "Date of first publication or broadcast"@en ;
                                  rdfs:label "schema:datePublished"@en .


###  http://schema.org/description
<http://schema.org/description> rdf:type owl:DatatypeProperty ;
                                rdfs:range rdf:langString ;
                                rdfs:comment "A description of the item"@en ;
                                rdfs:label "schema:description"@en .


###  http://schema.org/encodingFormat
<http://schema.org/encodingFormat> rdf:type owl:DatatypeProperty .


###  http://schema.org/endDate
<http://schema.org/endDate> rdf:type owl:DatatypeProperty ;
                            rdfs:range xsd:date ;
                            rdfs:comment "The end date and time of the item"@en ;
                            rdfs:label "schema:endDate"@en .


###  http://schema.org/familyName
<http://schema.org/familyName> rdf:type owl:DatatypeProperty ;
                               rdfs:range xsd:string ;
                               rdfs:comment "Family name. In the U.S., the last name of a Person"@en ;
                               rdfs:label "schema:familyName"@en .


###  http://schema.org/givenName
<http://schema.org/givenName> rdf:type owl:DatatypeProperty ;
                              rdfs:range xsd:string ;
                              rdfs:comment "Given name. In the U.S., the first name of a Person"@en ;
                              rdfs:label "schema:givenName"@en .


###  http://schema.org/headline
<http://schema.org/headline> rdf:type owl:DatatypeProperty .


###  http://schema.org/name
<http://schema.org/name> rdf:type owl:DatatypeProperty ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( <http://schema.org/DefinedTerm>
                                                     <http://schema.org/Place>
                                                     <http://xmlns.com/foaf/0.1/Organization>
                                                     <http://xmlns.com/foaf/0.1/Person>
                                                   )
                                     ] ;
                         rdfs:range xsd:string ;
                         rdfs:label "schema:name"@en .


###  http://schema.org/startDate
<http://schema.org/startDate> rdf:type owl:DatatypeProperty ;
                              rdfs:range xsd:date ;
                              rdfs:comment "The start date and time of the item"@en ;
                              rdfs:label "schema:startDate"@en .


###  http://schema.org/temporalCoverage
<http://schema.org/temporalCoverage> rdf:type owl:DatatypeProperty ;
                                     rdfs:domain :Document ;
                                     rdfs:range <http://schema.org/DateTime> ;
                                     rdfs:label "schema:temporalCoverage" .


###  https://gotriple.eu/ontology/triple#isDiscarded
:isDiscarded rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:boolean ;
             rdfs:comment "A boolean flag indicating whether an entity (author or keyword) has been marked as discarded and should not be used for search/filtering."@en ;
             rdfs:label "is discarded"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/spar/datacite/Identifier
<http://purl.org/spar/datacite/Identifier> rdf:type owl:Class ;
                                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://purl.org/spar/datacite/usesIdentifierScheme> ;
                                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                             owl:onClass <http://purl.org/spar/datacite/IdentifierScheme>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://purl.org/spar/literal/hasLiteralValue> ;
                                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                             owl:onDataRange xsd:string
                                                           ] ;
                                           rdfs:label "datacite:Identifier"@en .


###  http://purl.org/spar/datacite/IdentifierScheme
<http://purl.org/spar/datacite/IdentifierScheme> rdf:type owl:Class ;
                                                 rdfs:label "datacite:IdentifierScheme"@en .


###  http://schema.org/CreativeWork
<http://schema.org/CreativeWork> rdf:type owl:Class ;
                                 rdfs:label "schema:CreativeWork"@en .


###  http://schema.org/DefinedTerm
<http://schema.org/DefinedTerm> rdf:type owl:Class ;
                                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                  owl:onProperty <http://schema.org/name> ;
                                                  owl:someValuesFrom <http://schema.org/Text>
                                                ] ;
                                rdfs:comment "A word, name, acronym, phrase, etc. with a formal definition"@en ;
                                rdfs:label "schema:DefinedTerm"@en .


###  http://schema.org/Grant
<http://schema.org/Grant> rdf:type owl:Class ;
                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://schema.org/funder> ;
                                            owl:allValuesFrom [ rdf:type owl:Class ;
                                                                owl:unionOf ( <http://xmlns.com/foaf/0.1/Organization>
                                                                              <http://xmlns.com/foaf/0.1/Person>
                                                                            )
                                                              ]
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://schema.org/sponsor> ;
                                            owl:allValuesFrom [ rdf:type owl:Class ;
                                                                owl:unionOf ( <http://xmlns.com/foaf/0.1/Organization>
                                                                              <http://xmlns.com/foaf/0.1/Person>
                                                                            )
                                                              ]
                                          ] ;
                          rdfs:comment "A grant, typically financial or otherwise quantifiable, of resources"@en ;
                          rdfs:label "schema:Grant"@en .


###  http://schema.org/Language
<http://schema.org/Language> rdf:type owl:Class ;
                             rdfs:label "schema:Language"@en .


###  http://schema.org/Person
<http://schema.org/Person> rdf:type owl:Class ;
                           owl:equivalentClass <http://xmlns.com/foaf/0.1/Person> .


###  http://schema.org/Place
<http://schema.org/Place> rdf:type owl:Class ;
                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://schema.org/name> ;
                                            owl:someValuesFrom <http://schema.org/Text>
                                          ] ;
                          rdfs:label "schema:Place"@en .


###  http://schema.org/Project
<http://schema.org/Project> rdf:type owl:Class ;
                            rdfs:subClassOf [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://purl.org/spar/datacite/hasIdentifier> ;
                                              owl:allValuesFrom <http://purl.org/spar/datacite/Identifier>
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://schema.org/about> ;
                                              owl:allValuesFrom <http://www.w3.org/2004/02/skos/core#Concept>
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://schema.org/funding> ;
                                              owl:allValuesFrom <http://schema.org/Grant>
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://schema.org/keywords> ;
                                              owl:allValuesFrom <http://schema.org/DefinedTerm>
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://schema.org/alternateName> ;
                                              owl:allValuesFrom rdf:langString
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://schema.org/description> ;
                                              owl:allValuesFrom rdf:langString
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://schema.org/name> ;
                                              owl:minCardinality "1"^^xsd:nonNegativeInteger
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://schema.org/endDate> ;
                                              owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://schema.org/startDate> ;
                                              owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                            ] ;
                            rdfs:comment "A research project in the Social Sciences and Humanities domain integrated into the GoTriple platform"@en ;
                            rdfs:label "schema:Project"@en .


###  http://schema.org/Thing
<http://schema.org/Thing> rdf:type owl:Class ;
                          rdfs:label "schema:Thing"@en .


###  http://www.w3.org/2004/02/skos/core#Concept
