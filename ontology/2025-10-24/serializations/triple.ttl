@prefix : <https://gotriple.eu/ontology/triple#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://gotriple.eu/ontology/triple#> .

<https://gotriple.eu/ontology/triple> rdf:type owl:Ontology ;
                                       owl:versionIRI <https://gotriple.eu/ontology/triple/2.0.0> ;
                                       <http://purl.org/dc/terms/bibliographicCitation> "Bertozzi, A., De Santis, L., & Peroni, S. (2025). TRIPLE Ontology - GoTriple Platform Data Model (Version 2.0.0). GoTriple. https://gotriple.eu/ontology/triple" ;
                                       <http://purl.org/dc/terms/contributor> "Luca De Santis" ,
                                                                              "Silvio Peroni" ;
                                       <http://purl.org/dc/terms/created> "2025-10-06" ;
                                       <http://purl.org/dc/terms/creator> "Alessandro Bertozzi" ;
                                       <http://purl.org/dc/terms/description> "This ontology provides a formal model for representing scholarly resources in the Social Sciences and Humanities domain within the GoTriple discovery platform. It includes classes and properties for documents, authors, projects, identifiers, and their relationships."@en ;
                                       <http://purl.org/dc/terms/format> "application/rdf+xml" ,
                                                                         "text/turtle" ;
                                       <http://purl.org/dc/terms/language> "en" ;
                                       <http://purl.org/dc/terms/license> <https://creativecommons.org/licenses/by/4.0/> ;
                                       <http://purl.org/dc/terms/modified> "2025-10-24" ;
                                       <http://purl.org/dc/terms/publisher> "GoTriple" ;
                                       <http://purl.org/dc/terms/rights> "This work is licensed under Creative Commons Attribution 4.0 International License" ;
                                       <http://purl.org/dc/terms/source> <https://github.com/GoTriple/triple-ontology> ;
                                       <http://purl.org/dc/terms/subject> "Research Data" ,
                                                                          "Scholarly Communication" ,
                                                                          "Semantic Web" ,
                                                                          "Social Sciences and Humanities" ;
                                       <http://purl.org/dc/terms/title> "TRIPLE Ontology - GoTriple Platform Data Model"@en ;
                                       <http://purl.org/vocab/vann/preferredNamespacePrefix> "triple" ;
                                       <http://purl.org/vocab/vann/preferredNamespaceUri> "https://gotriple.eu/ontology/triple#" ;
                                       <http://schema.org/citation> "Bertozzi, A., De Santis, L., & Peroni, S. (2025). TRIPLE Ontology - GoTriple Platform Data Model (Version 2.0.0). GoTriple. https://gotriple.eu/ontology/triple" ;
                                       <http://schema.org/url> <https://gotriple.eu/ontology/triple> ;
                                       <http://schema.org/version> "2.0.0" ;
                                       rdfs:comment "Comprehensive semantic representation of the GoTriple discovery platform's data model for Social Sciences and Humanities (SSH) research artifacts."@en ;
                                       rdfs:label "TRIPLE Ontology"@en ;
                                       rdfs:seeAlso <https://gotriple.eu/> ,
                                                    <https://www.samod.org/> ;
                                       owl:priorVersion <https://gotriple.eu/ontology/triple/1.0.0> ;
                                       owl:versionInfo "2.0.0" ;
                                       <http://www.w3.org/ns/dcat#keyword> "humanities" ,
                                                                           "ontology" ,
                                                                           "research data" ,
                                                                           "semantic web" ,
                                                                           "social sciences" .

#################################################################
