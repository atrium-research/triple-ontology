    "http://purl.org/dc/terms/description": [
      {
        "@language": "en",
        "@value": "The Profile module models the actors within the GoTriple ecosystem. It defines `triple:Profile` for describing researchers and organizations, aligning them with `foaf:Person` and `foaf:Organization`. It also manages User Accounts (`foaf:OnlineAccount`) and their association with profiles to handle platform access and identity."
      }
    ],
    "http://purl.org/dc/terms/modified": [
      {
        "@value": "2025-12-22"
      }
