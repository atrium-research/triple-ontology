  <owl:Ontology rdf:about="https://gotriple.eu/ontology/triple/MediaObject">
    <rdfs:label xml:lang="en">TRIPLE Media Object Module</rdfs:label>
    <dcterms:title xml:lang="en">TRIPLE Media Object Module</dcterms:title>
    <dcterms:abstract xml:lang="en">The Media Object module of the TRIPLE Ontology, defining Audio, Video, and Image resources.</dcterms:abstract>
    <dcterms:description xml:lang="en">The Media Object module manages multimedia resources within the platform. It defines the `triple:MediaObject` class for audio, video, and image content, providing properties to describe their technical characteristics (duration, size, encoding format) and intellectual content, aligned with `schema:MediaObject` and `fabio:Work`.</dcterms:description>
    <dcterms:created>2021-12-01</dcterms:created>
    <dcterms:modified>2025-12-22</dcterms:modified>
    <dcterms:creator>Alessandro Bertozzi</dcterms:creator>
    <dcterms:contributor>Luca De Santis</dcterms:contributor>
    <dcterms:contributor>Silvio Peroni</dcterms:contributor>
    <dcterms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
    <dcterms:publisher rdf:resource="https://www.gotriple.eu/"/>
    <owl:versionInfo>2.0.0</owl:versionInfo>
    <schema:creativeWorkStatus>Ontology Module</schema:creativeWorkStatus>
  </owl:Ontology>
