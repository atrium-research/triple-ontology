<https://gotriple.eu/ontology/triple/ProjectType> a owl:Ontology ;
    rdfs:label "TRIPLE Project Types Vocabulary"@en ;
    dcterms:title "TRIPLE Project Types Vocabulary"@en ;
    dcterms:abstract "The Project Types vocabulary of the TRIPLE Ontology."@en ;
    dcterms:description "Defines the types of projects represented in the GoTriple platform (e.g., European Project, National Project)."@en ;
    dcterms:created "2021-12-01" ;
    dcterms:modified "2025-12-22" ;
    dcterms:creator "Alessandro Bertozzi" ;
    dcterms:contributor "Luca De Santis", "Silvio Peroni" ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
    dcterms:publisher <https://www.gotriple.eu/> ;
    owl:versionInfo "2.0.0" ;
    schema:creativeWorkStatus "Project_Types documentation" .

triple:ProjectType a owl:Class ;
    rdfs:label "Project Type"@en ;
    rdfs:subClassOf skos:Concept .

pt: a owl:NamedIndividual, triple:ProjectType ;
    rdfs:label "Project Type"@en .
