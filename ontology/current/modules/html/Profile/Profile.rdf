      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction rdf:nodeID="n69b215408fc642e390ac6fd9ded4c85ab2">
        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
