  <owl:Ontology rdf:about="https://gotriple.eu/ontology/triple/Profile">
    <rdfs:label xml:lang="en">TRIPLE Profile Module</rdfs:label>
    <dcterms:title xml:lang="en">TRIPLE Profile Module</dcterms:title>
    <dcterms:abstract xml:lang="en">The Profile module of the TRIPLE Ontology, defining Agents (People, Organizations) and Accounts.</dcterms:abstract>
    <dcterms:description xml:lang="en">The Profile module models the actors within the GoTriple ecosystem. It defines `triple:Profile` for describing researchers and organizations, aligning them with `foaf:Person` and `foaf:Organization`. It also manages User Accounts (`foaf:OnlineAccount`) and their association with profiles to handle platform access and identity.</dcterms:description>
    <dcterms:created>2021-12-01</dcterms:created>
    <dcterms:modified>2025-12-22</dcterms:modified>
    <dcterms:creator>Alessandro Bertozzi</dcterms:creator>
    <dcterms:contributor>Luca De Santis</dcterms:contributor>
    <dcterms:contributor>Silvio Peroni</dcterms:contributor>
    <dcterms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
    <dcterms:publisher rdf:resource="https://www.gotriple.eu/"/>
    <owl:versionInfo>2.0.0</owl:versionInfo>
    <schema:creativeWorkStatus>Ontology Module</schema:creativeWorkStatus>
  </owl:Ontology>
