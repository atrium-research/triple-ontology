@prefix : <https://gotriple.eu/ontology/triple#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <http://schema.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix triple: <https://gotriple.eu/ontology/triple#> .
@prefix datacite: <http://purl.org/spar/datacite/> .
@prefix litre: <http://purl.org/spar/literal/> .
@base <https://gotriple.eu/ontology/triple#> .

<https://gotriple.eu/ontology/triple> rdf:type owl:Ontology ;
                                       owl:versionIRI <https://gotriple.eu/ontology/triple/1.0.0> ;
                                       rdfs:label "TRIPLE Ontology"@en ;
                                       rdfs:comment "Comprehensive semantic representation of the GoTriple discovery platform's data model for Social Sciences and Humanities (SSH) research artifacts."@en ;
                                       dcterms:created "2025-10-06" ;
                                       dcterms:modified "2025-10-24" .

#################################################################
#    Datatypes
#################################################################

###  http://schema.org/DateTime
schema:DateTime rdf:type rdfs:Datatype .


###  http://schema.org/Text
schema:Text rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/elements/1.1/subject
dc:subject rdf:type owl:ObjectProperty ;
           rdfs:label "subject"@en ;
           rdfs:comment "The subject or topic of the document, using a term from a controlled vocabulary."@en .


###  http://purl.org/dc/elements/1.1/type
dc:type rdf:type owl:ObjectProperty ;
        rdfs:label "type"@en ;
        rdfs:comment "The type of the document (e.g., Article, Dataset, Book)."@en .


###  http://purl.org/spar/datacite/hasIdentifier
datacite:hasIdentifier rdf:type owl:ObjectProperty ;
                       rdfs:label "has identifier"@en .


###  http://purl.org/spar/datacite/usesIdentifierScheme
datacite:usesIdentifierScheme rdf:type owl:ObjectProperty ;
                              rdfs:label "uses identifier scheme"@en .


###  http://schema.org/about
schema:about rdf:type owl:ObjectProperty ;
             rdfs:label "about"@en ;
             rdfs:comment "The subject matter of the content"@en .


###  http://schema.org/author
schema:author rdf:type owl:ObjectProperty ;
              rdfs:label "author"@en ;
              rdfs:comment "The author of this content"@en ;
              rdfs:domain triple:Document ;
              rdfs:range [ owl:unionOf (foaf:Person foaf:Organization) ] ;
              rdfs:subPropertyOf schema:creator .


###  http://schema.org/conditionsOfAccess
schema:conditionsOfAccess rdf:type owl:ObjectProperty ;
                          rdfs:label "conditions of access"@en ;
                          rdfs:comment "The conditions under which the document can be accessed (e.g., Open Access, Restricted)."@en .


###  http://schema.org/contributor
schema:contributor rdf:type owl:ObjectProperty ;
                   rdfs:label "contributor"@en ;
                   rdfs:comment "A secondary contributor to the CreativeWork"@en ;
                   rdfs:domain triple:Document ;
                   rdfs:range [ owl:unionOf (foaf:Person foaf:Organization) ] .


###  http://schema.org/funder
schema:funder rdf:type owl:ObjectProperty ;
              rdfs:label "funder"@en ;
              rdfs:comment "A person or organization that supports something through some kind of financial contribution"@en ;
              rdfs:domain triple:Document ;
              rdfs:range [ owl:unionOf (foaf:Person foaf:Organization) ] .


###  http://schema.org/funding
schema:funding rdf:type owl:ObjectProperty ;
               rdfs:label "funding"@en ;
               rdfs:comment "A grant, typically financial or otherwise quantifiable, of resources"@en .


###  http://schema.org/inLanguage
schema:inLanguage rdf:type owl:ObjectProperty .


###  http://schema.org/keywords
schema:keywords rdf:type owl:ObjectProperty ;
                rdfs:label "keywords"@en ;
                rdfs:comment "Keywords or tags used to describe this content"@en .


###  http://schema.org/license
schema:license rdf:type owl:ObjectProperty ;
               rdfs:label "license"@en ;
               rdfs:comment "The license under which the document is published (e.g., CC-BY, CC0)."@en .


###  http://schema.org/mentions
schema:mentions rdf:type owl:ObjectProperty .


###  http://schema.org/producer
schema:producer rdf:type owl:ObjectProperty ;
                rdfs:label "producer"@en ;
                rdfs:comment "The person or organization who produced the work"@en ;
                rdfs:domain triple:Document ;
                rdfs:range [ owl:unionOf (foaf:Person foaf:Organization) ] .


###  http://schema.org/provider
schema:provider rdf:type owl:ObjectProperty ;
                rdfs:label "provider"@en ;
                rdfs:comment "The service provider, organization, or individual that provides the data"@en ;
                rdfs:domain triple:Document ;
                rdfs:range [ owl:unionOf (foaf:Person foaf:Organization) ] .


###  http://schema.org/publisher
schema:publisher rdf:type owl:ObjectProperty ;
                 rdfs:label "publisher"@en ;
                 rdfs:comment "The publisher of the creative work"@en ;
                 rdfs:domain triple:Document ;
                 rdfs:range [ owl:unionOf (foaf:Person foaf:Organization) ] .


###  http://schema.org/spatialCoverage
schema:spatialCoverage rdf:type owl:ObjectProperty ;
                       rdfs:domain triple:Document ;
                       rdfs:range schema:Place ;
                       rdfs:label "spatial coverage" .


###  http://schema.org/sponsor
schema:sponsor rdf:type owl:ObjectProperty ;
               rdfs:label "sponsor"@en ;
               rdfs:comment "A person or organization that supports a thing through a pledge, promise, or financial contribution"@en .


###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdf:type owl:ObjectProperty ;
                rdfs:label "close match"@en ;
                rdfs:comment "A SKOS property indicating that two concepts are sufficiently similar to be used interchangeably in some contexts."@en .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:ObjectProperty ;
                rdfs:label "exact match"@en ;
                rdfs:comment "A SKOS property indicating that two concepts can be used interchangeably across all contexts."@en .


###  http://www.w3.org/2004/02/skos/core#inScheme
skos:inScheme rdf:type owl:ObjectProperty ;
              rdfs:label "in scheme"@en ;
              rdfs:comment "A SKOS property linking a concept to the concept scheme it belongs to."@en .


###  http://xmlns.com/foaf/0.1/account
foaf:account rdf:type owl:ObjectProperty ;
             rdfs:label "account"@en ;
             rdfs:comment "Indicates an account held by this agent"@en .


###  https://gotriple.eu/ontology/triple#aggregator
triple:aggregator rdf:type owl:ObjectProperty ;
                  rdfs:label "aggregator"@en ;
                  rdfs:comment "An aggregator that collates documents from different sources"@en ;
                  rdfs:domain triple:Document ;
                  rdfs:range [ owl:unionOf (foaf:Person foaf:Organization) ] ;
                  rdfs:subPropertyOf schema:contributor .


###  https://gotriple.eu/ontology/triple#alsoKnownAs
triple:alsoKnownAs rdf:type owl:ObjectProperty ;
                   rdfs:label "also known as"@en ;
                   rdfs:comment "Links an ambiguous profile to the original profile"@en ;
                   rdfs:domain triple:Profile ;
                   rdfs:range triple:Profile .


###  https://gotriple.eu/ontology/triple#inCluster
triple:inCluster rdf:type owl:ObjectProperty ;
                 rdfs:label "in cluster"@en ;
                 rdfs:comment "Associates a document with its cluster (for deduplication purposes)."@en ;
                 rdfs:domain triple:Document ;
                 rdfs:range triple:Cluster .


###  https://gotriple.eu/ontology/triple#primaryProducer
triple:primaryProducer rdf:type owl:ObjectProperty ;
                       rdfs:label "primary producer"@en ;
                       rdfs:comment "The original source or creator of the document"@en ;
                       rdfs:domain triple:Document ;
                       rdfs:range [ owl:unionOf (foaf:Person foaf:Organization) ] ;
                       rdfs:subPropertyOf schema:producer .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/spar/literal/hasLiteralValue
litre:hasLiteralValue rdf:type owl:DatatypeProperty .


###  http://schema.org/abstract
schema:abstract rdf:type owl:DatatypeProperty .


###  http://schema.org/alternateName
schema:alternateName rdf:type owl:DatatypeProperty ;
                     rdfs:label "alternate name"@en ;
                     rdfs:comment "An alias for the item"@en ;
                     rdfs:range rdf:langString .


###  http://schema.org/datePublished
schema:datePublished rdf:type owl:DatatypeProperty ;
                     rdfs:label "date published"@en ;
                     rdfs:comment "Date of first publication or broadcast"@en ;
                     rdfs:domain triple:Document ;
                     rdfs:range xsd:date .


###  http://schema.org/description
schema:description rdf:type owl:DatatypeProperty ;
                   rdfs:label "description"@en ;
                   rdfs:comment "A description of the item"@en ;
                   rdfs:range rdf:langString .


###  http://schema.org/encodingFormat
schema:encodingFormat rdf:type owl:DatatypeProperty .


###  http://schema.org/endDate
schema:endDate rdf:type owl:DatatypeProperty ;
               rdfs:label "end date"@en ;
               rdfs:comment "The end date and time of the item"@en ;
               rdfs:range xsd:date .


###  http://schema.org/familyName
schema:familyName rdf:type owl:DatatypeProperty ;
                  rdfs:label "family name"@en ;
                  rdfs:comment "Family name. In the U.S., the last name of a Person"@en ;
                  rdfs:range xsd:string .


###  http://schema.org/givenName
schema:givenName rdf:type owl:DatatypeProperty ;
                 rdfs:label "given name"@en ;
                 rdfs:comment "Given name. In the U.S., the first name of a Person"@en ;
                 rdfs:range xsd:string .


###  http://schema.org/headline
schema:headline rdf:type owl:DatatypeProperty .


###  http://schema.org/name
schema:name rdf:type owl:DatatypeProperty ;
            rdfs:label "name"@en ;
            rdfs:domain [ owl:unionOf (foaf:Person foaf:Organization schema:Place schema:DefinedTerm) ] ;
            rdfs:range xsd:string .


###  http://schema.org/startDate
schema:startDate rdf:type owl:DatatypeProperty ;
                 rdfs:label "start date"@en ;
                 rdfs:comment "The start date and time of the item"@en ;
                 rdfs:range xsd:date .


###  http://schema.org/temporalCoverage
schema:temporalCoverage rdf:type owl:DatatypeProperty ;
                        rdfs:domain triple:Document ;
                        rdfs:range schema:DateTime ;
                        rdfs:label "temporal coverage" .


###  https://gotriple.eu/ontology/triple#isDiscarded
triple:isDiscarded rdf:type owl:DatatypeProperty ;
                   rdfs:label "is discarded"@en ;
                   rdfs:comment "A boolean flag indicating whether an entity (author or keyword) has been marked as discarded and should not be used for search/filtering."@en ;
                   rdfs:range xsd:boolean .


#################################################################
#    Annotation Properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty ;
                rdfs:label "definition"@en ;
                rdfs:comment "A SKOS annotation property providing a complete explanation of the intended meaning of a concept."@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/spar/datacite/Identifier
datacite:Identifier rdf:type owl:Class ;
                    rdfs:label "Identifier (DataCite)"@en ;
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty datacite:usesIdentifierScheme ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass datacite:IdentifierScheme
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty litre:hasLiteralValue ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange xsd:string
                                    ] .


###  http://purl.org/spar/datacite/IdentifierScheme
datacite:IdentifierScheme rdf:type owl:Class ;
                          rdfs:label "Identifier Scheme (DataCite)"@en .


###  http://schema.org/CreativeWork
schema:CreativeWork rdf:type owl:Class ;
                    rdfs:label "Creative Work (Schema.org)"@en .


###  http://schema.org/DefinedTerm
schema:DefinedTerm rdf:type owl:Class ;
                   rdfs:label "Defined Term (Schema.org)"@en ;
                   rdfs:comment "A word, name, acronym, phrase, etc. with a formal definition"@en ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty schema:name ;
                                     owl:someValuesFrom schema:Text
                                   ] .


###  http://schema.org/Grant
schema:Grant rdf:type owl:Class ;
             rdfs:label "Grant (Schema.org)"@en ;
             rdfs:comment "A grant, typically financial or otherwise quantifiable, of resources"@en ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:funder ;
                               owl:allValuesFrom [ owl:unionOf (foaf:Person foaf:Organization) ]
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty schema:sponsor ;
                               owl:allValuesFrom [ owl:unionOf (foaf:Person foaf:Organization) ]
                             ] .


###  http://schema.org/Language
schema:Language rdf:type owl:Class ;
                rdfs:label "Language (Schema.org)"@en .


###  http://schema.org/Place
schema:Place rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:name ;
                               owl:someValuesFrom schema:Text
                             ] ;
             rdfs:label "Place (Schema.org)"@en .


###  http://schema.org/Project
schema:Project rdf:type owl:Class ;
               rdfs:label "Project (Schema.org)"@en ;
               rdfs:comment "A research project in the Social Sciences and Humanities domain integrated into the GoTriple platform"@en ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:name ;
                                 owl:minCardinality "1"^^xsd:nonNegativeInteger
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty datacite:hasIdentifier ;
                                 owl:allValuesFrom datacite:Identifier
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:startDate ;
                                 owl:maxCardinality "1"^^xsd:nonNegativeInteger
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:endDate ;
                                 owl:maxCardinality "1"^^xsd:nonNegativeInteger
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:funding ;
                                 owl:allValuesFrom schema:Grant
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:about ;
                                 owl:allValuesFrom skos:Concept
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:keywords ;
                                 owl:allValuesFrom schema:DefinedTerm
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:description ;
                                 owl:allValuesFrom rdf:langString
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:alternateName ;
                                 owl:allValuesFrom rdf:langString
                               ] .


###  http://schema.org/Thing
schema:Thing rdf:type owl:Class ;
             rdfs:label "Thing (Schema.org)"@en .


###  http://www.w3.org/2004/02/skos/core#Concept
skos:Concept rdf:type owl:Class ;
             rdfs:label "Concept (SKOS)"@en ;
             rdfs:comment "A SKOS concept representing a term in a controlled vocabulary."@en .


###  http://www.w3.org/2004/02/skos/core#ConceptScheme
skos:ConceptScheme rdf:type owl:Class ;
                   rdfs:label "Concept Scheme (SKOS)"@en ;
                   rdfs:comment "A SKOS concept scheme representing a controlled vocabulary (e.g., licenses, document types, disciplines)."@en .


###  http://xmlns.com/foaf/0.1/Document
foaf:Document rdf:type owl:Class ;
              rdfs:label "Document (FOAF)"@en .


###  http://xmlns.com/foaf/0.1/OnlineAccount
foaf:OnlineAccount rdf:type owl:Class ;
                   rdfs:label "Online Account (FOAF)"@en ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty datacite:hasIdentifier ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass datacite:Identifier
                                   ] .


###  http://xmlns.com/foaf/0.1/Organization
foaf:Organization rdf:type owl:Class ;
                  rdfs:label "Organization (FOAF)"@en ;
                  rdfs:subClassOf schema:Thing ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty schema:name ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onDataRange xsd:string
                                  ] .


###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class ;
            rdfs:label "Person (FOAF)"@en ;
            owl:equivalentClass schema:Person ;
            rdfs:subClassOf schema:Thing ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty schema:name ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] .


###  https://gotriple.eu/ontology/triple#Cluster
triple:Cluster rdf:type owl:Class ;
               rdfs:label "Cluster"@en ;
               rdfs:comment "A cluster groups multiple documents from different sources that represent the same scholarly work. Used for deduplication."@en ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty datacite:hasIdentifier ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass datacite:Identifier
                               ] .


###  https://gotriple.eu/ontology/triple#Document
triple:Document rdf:type owl:Class ;
                rdfs:label "Document (Triple)"@en ;
                rdfs:comment "A document in the GoTriple platform, representing scholarly publications, research datasets, and other SSH research artifacts. It is a subclass of both schema:CreativeWork and foaf:Document."@en ;
                rdfs:subClassOf schema:CreativeWork ,
                                foaf:Document ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty dc:type ;
                                  owl:someValuesFrom skos:Concept
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty datacite:hasIdentifier ;
                                  owl:someValuesFrom datacite:Identifier
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:inLanguage ;
                                  owl:someValuesFrom schema:Language
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:headline ;
                                  owl:someValuesFrom rdf:langString
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:abstract ;
                                  owl:someValuesFrom rdf:langString
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:encodingFormat ;
                                  owl:someValuesFrom xsd:string
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:conditionsOfAccess ;
                                  owl:someValuesFrom skos:Concept
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:author ;
                                  owl:allValuesFrom triple:Profile
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:contributor ;
                                  owl:allValuesFrom [ owl:unionOf (foaf:Person foaf:Organization) ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:publisher ;
                                  owl:maxCardinality 1 ;
                                  owl:allValuesFrom [ owl:unionOf (foaf:Person foaf:Organization) ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:producer ;
                                  owl:allValuesFrom [ owl:unionOf (foaf:Person foaf:Organization) ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:funder ;
                                  owl:allValuesFrom [ owl:unionOf (foaf:Person foaf:Organization) ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:provider ;
                                  owl:allValuesFrom [ owl:unionOf (foaf:Person foaf:Organization) ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty triple:aggregator ;
                                  owl:allValuesFrom [ owl:unionOf (foaf:Person foaf:Organization) ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty triple:primaryProducer ;
                                  owl:allValuesFrom [ owl:unionOf (foaf:Person foaf:Organization) ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:keywords ;
                                  owl:someValuesFrom schema:DefinedTerm
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:spatialCoverage ;
                                  owl:someValuesFrom schema:Place
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:temporalCoverage ;
                                  owl:someValuesFrom schema:DateTime
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty triple:inCluster ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass triple:Cluster
                                ] .


###  https://gotriple.eu/ontology/triple#Profile
triple:Profile rdf:type owl:Class ;
               rdfs:label "profile"@en ;
               rdfs:comment "An author profile representing a person in the GoTriple system. Can be claimed or unclaimed."@en ;
               rdfs:subClassOf foaf:Person ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty foaf:account ;
                                 owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass foaf:OnlineAccount
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty triple:alsoKnownAs ;
                                 owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass triple:Profile
                               ] .


#################################################################
#    Individuals
#################################################################

###  https://gotriple.eu/ontology/triple#document_types
triple:document_types rdf:type owl:NamedIndividual ,
                               skos:ConceptScheme ;
                      rdfs:label "Document Types"@en ;
                      rdfs:comment "Controlled vocabulary of document types used in GoTriple"@en .


###  https://gotriple.eu/ontology/triple#doi
triple:doi rdf:type owl:NamedIndividual ,
                    datacite:IdentifierScheme ;
           rdfs:label "DOI"@en ;
           rdfs:comment "Digital Object Identifier - a persistent identifier for digital objects maintained by the International DOI Foundation."@en .


###  https://gotriple.eu/ontology/triple#issn
triple:issn rdf:type owl:NamedIndividual ,
                     datacite:IdentifierScheme ;
            rdfs:label "ISSN"@en ;
            rdfs:comment "International Standard Serial Number - an eight-digit identifier for serial publications."@en .


###  https://gotriple.eu/ontology/triple#isbn
triple:isbn rdf:type owl:NamedIndividual ,
                     datacite:IdentifierScheme ;
            rdfs:label "ISBN"@en ;
            rdfs:comment "International Standard Book Number - a numeric identifier for books and book-like products."@en .


###  https://gotriple.eu/ontology/triple#handle
triple:handle rdf:type owl:NamedIndividual ,
                       datacite:IdentifierScheme ;
              rdfs:label "Handle"@en ;
              rdfs:comment "Handle System - a persistent identifier infrastructure for managing digital objects."@en .


