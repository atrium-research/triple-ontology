@prefix datacite: <http://purl.org/spar/datacite/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix litre: <http://purl.org/spar/literal/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix triple: <https://gotriple.eu/ontology/triple#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dc:subject a owl:ObjectProperty .

<http://purl.org/spar/pro/author> a <http://purl.org/spar/pro/Role>,
        owl:NamedIndividual .

<http://purl.org/spar/pro/producer> a <http://purl.org/spar/pro/Role>,
        owl:NamedIndividual .

<http://purl.org/spar/pro/publisher> a <http://purl.org/spar/pro/Role>,
        owl:NamedIndividual .

schema1:Project a owl:Class .

schema1:license a owl:ObjectProperty .

schema1:mentions a owl:ObjectProperty .

skos:ConceptScheme a owl:Class .

skos:closeMatch a owl:ObjectProperty .

skos:exactMatch a owl:ObjectProperty .

skos:inScheme a owl:ObjectProperty .

foaf:Organization a owl:Class ;
    rdfs:subClassOf schema1:Thing,
        foaf:Agent .

<https://gotriple.eu/ontology/triple> a owl:Ontology ;
    rdfs:label "TRIPLE Ontology"@en ;
    dcterms:created "2025-10-06" ;
    dcterms:modified "2025-10-06" ;
    rdfs:comment "Comprehensive semantic representation of the GoTriple discovery platform's data model for Social Sciences and Humanities (SSH) research artifacts."@en ;
    owl:versionIRI <https://gotriple.eu/ontology/triple/1.0.0>,
        "https://gotriple.eu/ontology/triple/1.0.0" .

triple:aggregator a <http://purl.org/spar/pro/Role>,
        owl:NamedIndividual .

triple:contributor a <http://purl.org/spar/pro/Role>,
        owl:NamedIndividual .

triple:document_1 a owl:NamedIndividual,
        foaf:Document .

triple:document_2 a owl:NamedIndividual ;
    schema1:keywords triple:keyword_3 ;
    schema1:spatialCoverage triple:place_67 ;
    schema1:temporalCoverage "1942"^^schema1:DateTime .

triple:funder a <http://purl.org/spar/pro/Role>,
        owl:NamedIndividual .

triple:isDiscarded a owl:DatatypeProperty ;
    rdfs:range xsd:boolean .

triple:primaryProducer a <http://purl.org/spar/pro/Role>,
        owl:NamedIndividual ;
    rdfs:label "primary producer" .

triple:provider a <http://purl.org/spar/pro/Role>,
        owl:NamedIndividual .

datacite:IdentifierSchema a owl:Class ;
    rdfs:label "identifier schema" .

<http://purl.org/spar/dataciteIdentifier> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass <http://purl.org/spar/dataciteIdentifierScheme> ;
            owl:onProperty <http://purl.org/spar/dataciteusesIdentifierScheme> ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

<http://purl.org/spar/dataciteIdentifierScheme> a owl:Class .

<http://purl.org/spar/datacitehasIdentifier> a owl:ObjectProperty .

<http://purl.org/spar/dataciteusesIdentifierScheme> a owl:ObjectProperty .

litre:hasLiteralValue a owl:DatatypeProperty .

schema1:CreativeWork a owl:Class .

schema1:Language a owl:Class .

schema1:conditionsOfAccess a owl:ObjectProperty .

schema1:inLanguage a owl:ObjectProperty .

schema1:keywords a owl:ObjectProperty ;
    rdfs:label "keywords" .

schema1:spatialCoverage a owl:ObjectProperty ;
    rdfs:label "spatial coverage" ;
    rdfs:domain foaf:Document ;
    rdfs:range schema1:Place .

schema1:temporalCoverage a owl:DatatypeProperty ;
    rdfs:label "temporal coverage" ;
    rdfs:domain foaf:Document ;
    rdfs:range schema1:DateTime .

foaf:OnlineAccount a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass <http://purl.org/spar/dataciteIdentifier> ;
            owl:onProperty <http://purl.org/spar/datacitehasIdentifier> ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

foaf:Person a owl:Class ;
    rdfs:subClassOf schema1:Thing,
        foaf:Agent .

foaf:account a owl:ObjectProperty .

triple:Profile a owl:Class ;
    rdfs:label "profile" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass foaf:OnlineAccount ;
            owl:onProperty foaf:account ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass triple:Profile ;
            owl:onProperty triple:alsoKnowAs ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        foaf:Person .

triple:alsoKnowAs a owl:ObjectProperty ;
    rdfs:label "also know as" .

triple:inCluster a owl:ObjectProperty ;
    rdfs:label "in cluster" ;
    rdfs:domain foaf:Document ;
    rdfs:range triple:Cluster .

triple:isHeldBy a owl:ObjectProperty .

triple:keyword_56 a schema1:definedTerm,
        owl:NamedIndividual .

triple:keyword_6 a schema1:definedTerm,
        owl:NamedIndividual .

triple:place_67 a schema1:Place,
        owl:NamedIndividual ;
    schema1:name "place_67"^^schema1:DateTime .

datacite:hasIdentifer a owl:ObjectProperty .

datacite:usesIdentiferScheme a owl:ObjectProperty .

datacite:usesIdentifierScheme a owl:ObjectProperty ;
    rdfs:label "uses identifier scheme" .

<http://purl.org/spar/pro/isHeldBy> a owl:ObjectProperty ;
    rdfs:label "is held by" .

schema1:DateTime a rdfs:Datatype .

schema1:Text a rdfs:Datatype .

schema1:Thing a owl:Class .

schema1:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:domain schema1:Place,
        schema1:definedTerm .

<http://www.essepuntato.it/2012/04/tvc/atTime> a owl:ObjectProperty .

<http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval> a owl:Class ;
    rdfs:label "Time interval",
        "time interval" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#hasIntervalEndDate> ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#hasIntervalStartDate> ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#hasIntervalEndDate> ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#hasIntervalStartDate> ] .

<http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#hasIntervalEndDate> a owl:DatatypeProperty ;
    rdfs:label "interval has start date" .

<http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#hasIntervalStartDate> a owl:DatatypeProperty ;
    rdfs:label "interval has end date" .

triple:Cluster a owl:Class ;
    rdfs:label "cluster" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass datacite:Identifier ;
            owl:onProperty datacite:hasIdentifier ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

triple:keyword_3 a schema1:definedTerm,
        owl:NamedIndividual ;
    schema1:keywords triple:keyword_3,
        triple:keyword_56,
        triple:keyword_6 .

dc:type a owl:ObjectProperty .

datacite:IdentifierScheme a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass datacite:IdentifierScheme ;
            owl:onProperty datacite:usesIdentiferScheme ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass datacite:IdentifierScheme ;
            owl:onProperty datacite:usesIdentiferScheme ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

datacite:hasIdentifier a owl:ObjectProperty ;
    rdfs:label "has identifier" .

<http://purl.org/spar/pro/isDocumentContextFor> a owl:ObjectProperty ;
    rdfs:label "is document context for" .

<http://purl.org/spar/pro/withRole> a owl:ObjectProperty ;
    rdfs:label "with role" ;
    rdfs:domain <http://purl.org/spar/pro/RoleInTime> ;
    rdfs:range <http://purl.org/spar/pro/Role> .

foaf:name a owl:DatatypeProperty .

<http://purl.org/spar/pro/RoleInTime> a owl:Class ;
    rdfs:label "role in time" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <http://purl.org/spar/pro/withRole> ;
            owl:someValuesFrom <http://purl.org/spar/pro/Role> ],
        [ a owl:Restriction ;
            owl:onProperty <http://www.essepuntato.it/2012/04/tvc/atTime> ;
            owl:someValuesFrom <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval> ],
        [ a owl:Restriction ;
            owl:onClass foaf:Agent ;
            owl:onProperty triple:isHeldBy ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onProperty <http://purl.org/spar/pro/withRole> ;
            owl:someValuesFrom <http://purl.org/spar/pro/Role> ],
        [ a owl:Restriction ;
            owl:onProperty <http://www.essepuntato.it/2012/04/tvc/atTime> ;
            owl:someValuesFrom <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval> ],
        [ a owl:Restriction ;
            owl:onClass foaf:Agent ;
            owl:onProperty <http://purl.org/spar/pro/isHeldBy> ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onProperty <http://purl.org/spar/pro/withRole> ;
            owl:someValuesFrom <http://purl.org/spar/pro/Role> ],
        [ a owl:Restriction ;
            owl:onClass foaf:Agent ;
            owl:onProperty <http://purl.org/spar/pro/isHeldBy> ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

schema1:Place a owl:Class ;
    rdfs:label "place" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty schema1:name ;
            owl:someValuesFrom schema1:Text ] .

skos:Concept a owl:Class .

foaf:Document a owl:Class ;
    rdfs:label "document",
        "documents" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <http://purl.org/spar/pro/isDocumentContextFor> ;
            owl:someValuesFrom <http://purl.org/spar/pro/RoleInTime> ],
        [ a owl:Restriction ;
            owl:onProperty dc:type ;
            owl:someValuesFrom skos:Concept ],
        [ a owl:Restriction ;
            owl:onProperty datacite:hasIdentifier ;
            owl:someValuesFrom datacite:Identifier ],
        [ a owl:Restriction ;
            owl:onProperty schema1:inLanguage ;
            owl:someValuesFrom schema1:Language ],
        [ a owl:Restriction ;
            owl:onClass datacite:Identifier ;
            owl:onProperty datacite:hasIdentifier ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass triple:Cluster ;
            owl:onProperty triple:inCluster ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onProperty schema1:conditionsOfAccess ;
            owl:someValuesFrom skos:Concept ],
        [ a owl:Restriction ;
            owl:onProperty schema1:license ;
            owl:someValuesFrom skos:Concept ],
        [ a owl:Restriction ;
            owl:onProperty dc:subject ;
            owl:someValuesFrom skos:Concept ],
        [ a owl:Restriction ;
            owl:onProperty schema1:keywords ;
            owl:someValuesFrom schema1:definedTerm ],
        [ a owl:Restriction ;
            owl:onProperty schema1:spatialCoverage ;
            owl:someValuesFrom schema1:Place ],
        [ a owl:Restriction ;
            owl:onProperty schema1:temporalCoverage ;
            owl:someValuesFrom schema1:DateTime ],
        schema1:CreativeWork .

datacite:Identifier a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass datacite:IdentifierScheme ;
            owl:onProperty datacite:usesIdentifierScheme ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty litre:hasLiteralValue ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass datacite:IdentifierSchema ;
            owl:onProperty datacite:usesIdentifierScheme ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

schema1:definedTerm a owl:Class ;
    rdfs:label "defined term" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty schema1:name ;
            owl:someValuesFrom schema1:Text ] .

foaf:Agent a owl:Class ;
    rdfs:label "agent" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty foaf:name ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty foaf:name ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty foaf:name ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] .

<http://purl.org/spar/pro/Role> a owl:Class ;
    rdfs:label "role" .

