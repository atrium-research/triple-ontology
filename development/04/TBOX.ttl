#################################################################
#    Datatypes
#################################################################

###  https://schema.org/Text
<https://schema.org/Text> rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://schema.org/keywords
<https://schema.org/keywords> rdf:type owl:ObjectProperty ;
                             rdfs:label "schema:keywords" .


###  https://schema.org/spatialCoverage
<https://schema.org/spatialCoverage> rdf:type owl:ObjectProperty ;
                                    rdfs:range <https://schema.org/Place> ;
                                    rdfs:label "schema:spatialCoverage" .



#################################################################
#    Data properties
#################################################################

###  https://schema.org/name
<https://schema.org/name> rdf:type owl:DatatypeProperty ;
                         rdfs:label "schema:name" .


###  https://schema.org/temporalCoverage
<https://schema.org/temporalCoverage> rdf:type owl:DatatypeProperty ;
                                     rdfs:range xsd:string ;
                                     rdfs:label "schema:temporalCoverage" .


#################################################################
#    Classes
#################################################################

###  https://schema.org/Place
<https://schema.org/Place> rdf:type owl:Class ;
                          rdfs:subClassOf [ rdf:type owl:Restriction ;
                                            owl:onProperty <https://schema.org/name> ;
                                            owl:someValuesFrom xsd:string
                                          ] ;
                          rdfs:label "schema:Place"@en .



###  https://schema.org/DefinedTerm
<https://schema.org/DefinedTerm> rdf:type owl:Class ;
                                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                  owl:onProperty <https://schema.org/name> ;
                                                  owl:someValuesFrom <https://schema.org/Text>
                                                ] ;
                                rdfs:label "schema:DefinedTerm"@en .


###  https://schema.org/CreativeWork
schema:CreativeWork rdf:type owl:Class ;
                    rdfs:label "schema:CreativeWork"@en .


###  http://xmlns.com/foaf/0.1/Document
foaf:Document rdf:type owl:Class ;
              rdfs:label "foaf:Document"@en .


###  https://gotriple.eu/ontology/triple#Document
:Document rdf:type owl:Class ;
          rdfs:label "Document (Triple)"@en ;
          rdfs:comment "A document in the GoTriple platform, representing scholarly publications, research datasets, and other SSH research artifacts. It is a subclass of both schema:CreativeWork and foaf:Document."@en ;
          rdfs:subClassOf schema:CreativeWork ,
                          foaf:Document ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <https://schema.org/keywords> ;
                            owl:someValuesFrom <https://schema.org/DefinedTerm>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <https://schema.org/spatialCoverage> ;
                            owl:someValuesFrom <https://schema.org/Place>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <https://schema.org/temporalCoverage> ;
                            owl:someValuesFrom xsd:string
                          ] .


