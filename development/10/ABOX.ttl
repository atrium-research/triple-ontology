###  Topics
triple:topic-archaeology rdf:type skos:Concept ;
                         skos:prefLabel "Archaeology"@en ;
                         skos:definition "The study of human activity through the recovery and analysis of material culture."@en .

triple:topic-social-science rdf:type skos:Concept ;
                            skos:prefLabel "Social Science"@en ;
                            skos:definition "Academic disciplines concerned with society and the relationships among individuals within a society."@en .

###  Keywords (DefinedTerms)
triple:keyword-archaeology rdf:type schema:DefinedTerm ;
                          schema:name "archaeology"@en ;
                          schema:description "The study of human activity through material culture recovery and analysis"@en .

triple:keyword-europe rdf:type schema:DefinedTerm ;
                      schema:name "Europe"@en ;
                      schema:description "The European continent and its geographical regions"@en .

triple:keyword-heritage rdf:type schema:DefinedTerm ;
                        schema:name "heritage"@en ;
                        schema:description "Cultural and natural heritage of human societies"@en .

triple:keyword-spatial-data rdf:type schema:DefinedTerm ;
                            schema:name "spatial data"@en ;
                            schema:description "Data with geographical coordinates and spatial relationships"@en .

triple:keyword-social-attitudes rdf:type schema:DefinedTerm ;
                                schema:name "social attitudes"@en ;
                                schema:description "Individual and collective opinions on social issues"@en .

triple:keyword-survey rdf:type schema:DefinedTerm ;
                      schema:name "survey"@en ;
                      schema:description "Research methodology using questionnaires to collect data"@en .

triple:keyword-politics rdf:type schema:DefinedTerm ;
                        schema:name "politics"@en ;
                        schema:description "Activities related to governance and public affairs"@en .

triple:keyword-demographics rdf:type schema:DefinedTerm ;
                            schema:name "demographics"@en ;
                            schema:description "Statistical characteristics of populations"@en .

