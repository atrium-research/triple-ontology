###  Dataset Example 1: Geospatial Research Dataset
triple:dataset-001 rdf:type triple:Dataset ;
                   datacite:hasIdentifier triple:identifier-dataset-001 ;
                   schema:headline "European Archaeological Sites Database"@en ;
                   schema:abstract "A comprehensive dataset containing archaeological site information across Europe, including spatial coordinates, dating, and cultural attributions."@en ;
                   schema:author "Jane Smith", "Mario Rossi" ;
                   schema:contributor "Dr. Anna Fischer" ;
                   schema:dateCreated "2023-01-15"^^xsd:date ;
                   dcterms:provenance triple:provenance-heritage-001 ;
                   schema:datePublished "2023-03-01"^^xsd:date ;
                   schema:dateModified "2023-05-20"^^xsd:date ;
                   schema:publisher "European Heritage Institute" ;
                   schema:provider "ISIDORE" ;
                   dcat:bbox "POLYGON((-10 35, 30 35, 30 70, -10 70, -10 35))"^^rdfs:Literal ;
                   schema:keywords triple:keyword-archaeology, triple:keyword-europe, triple:keyword-heritage, triple:keyword-spatial-data ;

                   schema:inLanguage "en" ;
                   schema:spatialCoverage triple:place-europe ;
                   schema:temporalCoverage "2000 BCE - 1500 CE" ;
                   schema:encodingFormat "text/csv", "application/json" ;
                   schema:size "15.2 MB" ;
                   datacite:hasIdentifier triple:identifier-heritage-download ,
                                          triple:identifier-heritage-landing ,
                                          triple:identifier-heritage-internal ,
                                          triple:identifier-heritage-pid ,
                                          triple:identifier-heritage-original ,
                                          triple:identifier-heritage-doi ,
                                          triple:identifier-heritage-handle ;
                   schema:creativeWorkStatus "Published" ;
                   schema:contactPoint triple:contact-heritage-institute ;
                   schema:funding triple:project-heritage-mapping ;
                   sioc:topic triple:topic-archaeology ;
                   dcat:distribution triple:distribution-heritage-001 ;
                   schema:comment "Dataset created through systematic archaeological surveys across 15 European countries with standardized recording protocols."@en .

triple:distribution-heritage-001 rdf:type dcat:Distribution ;
                                 dcat:accessURL "https://data.heritage.eu/download/sites.csv" .

###  Dataset Example 2: Social Science Survey Data  
triple:dataset-002 rdf:type triple:Dataset ;
                   datacite:hasIdentifier triple:identifier-dataset-002 ;
                   schema:headline "European Social Attitudes Survey 2023"@en ;
                   schema:abstract "Annual survey data on social attitudes, political opinions, and demographic information from EU member states."@en ;
                   schema:author "Social Research Institute" ;
                   schema:contributor "Prof. Elena Rodriguez", "Dr. Klaus Mueller" ;
                   schema:dateCreated "2023-06-01"^^xsd:date ;
                   schema:datePublished "2023-09-15"^^xsd:date ;
                   schema:dateModified "2023-10-02"^^xsd:date ;
                   schema:publisher "EU Social Observatory" ;
                   schema:provider "OpenAIRE" ;
                   dcat:bbox "POLYGON((-10 34, 34 34, 34 72, -10 72, -10 34))"^^rdfs:Literal ;
                   schema:keywords triple:keyword-social-attitudes, triple:keyword-survey, triple:keyword-politics, triple:keyword-demographics ;

                   schema:knowsAbout "political science", "sociology", "public opinion" ;
                   schema:inLanguage "en", "de", "fr", "it" ;
                   dcterms:provenance triple:provenance-social-002 ;
                   schema:spatialCoverage triple:place-european-union ;
                   schema:temporalCoverage "2023" ;
                   schema:encodingFormat "application/x-spss", "text/csv" ;
                   schema:size "245 MB" ;
                   datacite:hasIdentifier triple:identifier-social-download ,
                                          triple:identifier-social-landing ,
                                          triple:identifier-social-internal ,
                                          triple:identifier-social-pid ,
                                          triple:identifier-social-original ,
                                          triple:identifier-social-doi ,
                                          triple:identifier-social-handle ;
                   schema:creativeWorkStatus "Published" ;
                   schema:contactPoint triple:contact-social-observatory ;
                   schema:funding triple:project-social-cohesion ;
                   sioc:topic triple:topic-social-science ;
                   schema:mentions triple:dataset-001 ;
                   schema:comment "Longitudinal survey methodology following EU standards for social research data collection and processing."@en ;
                   dcterms:isReferencedBy triple:document-social-report-2023 .

###  Provenance Statements
triple:provenance-heritage-001 rdf:type dcterms:ProvenanceStatement ;
                               rdfs:label "Provenance for Heritage Dataset 001"@en ;
                               rdfs:comment "Data aggregated from 15 national museums and normalized to CIDOC-CRM before conversion to CSV."@en .

triple:provenance-social-002 rdf:type dcterms:ProvenanceStatement ;
                             rdfs:label "Provenance for Social Dataset 002"@en ;
                             rdfs:comment "Survey results merged from 27 EU member state polls, anonymized, and weighted for demographic representativeness."@en .

