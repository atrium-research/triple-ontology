@prefix triple: <https://gotriple.eu/ontology/triple/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix fabio: <http://purl.org/spar/fabio/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix frapo: <http://purl.org/cerif/frapo/> .
#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdf:type owl:ObjectProperty .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://purl.org/cerif/frapo/Grant
frapo:Grant rdf:type owl:Class ;
            rdfs:label "frapo:Grant"@en ;
            rdfs:comment "A funding grant."@en .


###  http://purl.org/spar/fabio/Dataset
fabio:Dataset rdf:type owl:Class ;
              rdfs:label "fabio:Dataset"@en ;
              rdfs:comment "A dataset."@en .


###  http://purl.org/spar/fabio/ScholarlyWork
fabio:ScholarlyWork rdf:type owl:Class ;
                    rdfs:label "fabio:ScholarlyWork"@en ;
                    rdfs:comment "A scholarly work."@en .


###  http://purl.org/spar/fabio/Work
fabio:Work rdf:type owl:Class ;
           rdfs:label "fabio:Work"@en ;
           rdfs:comment "A work."@en .


###  http://xmlns.com/foaf/0.1/Agent
foaf:Agent rdf:type owl:Class ;
           rdfs:label "foaf:Agent"@en ;
           rdfs:comment "An agent (person or organization)."@en .


###  https://gotriple.eu/ontology/triple/Dataset
triple:Dataset rdf:type owl:Class ;
               skos:exactMatch fabio:Dataset .


###  https://gotriple.eu/ontology/triple/Document
triple:Document rdf:type owl:Class ;
                skos:exactMatch fabio:ScholarlyWork .


###  https://gotriple.eu/ontology/triple/MediaObject
triple:MediaObject rdf:type owl:Class ;
                   skos:closeMatch fabio:Work .


###  https://gotriple.eu/ontology/triple/Profile
triple:Profile rdf:type owl:Class ;
               skos:exactMatch foaf:Agent .


###  https://gotriple.eu/ontology/triple/Project
triple:Project rdf:type owl:Class ;
               skos:closeMatch frapo:Grant .


###  https://gotriple.eu/ontology/triple/SemanticArtefact
triple:SemanticArtefact rdf:type owl:Class ;
                        skos:closeMatch fabio:Work .
