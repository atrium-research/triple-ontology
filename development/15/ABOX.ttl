@prefix triple: <https://gotriple.eu/ontology/triple/> .
@prefix oa: <http://www.w3.org/ns/oa#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix schema: <https://schema.org/> .

#################################################################
#    Individuals
#################################################################

###  triple:Document1
triple:Document1 rdf:type owl:NamedIndividual ,
                          triple:Document ;
                 schema:mentions triple:ann_101 ,
                                 triple:ann_102 ,
                                 triple:ann_103 ,
                                 triple:ann_104 ,
                                 triple:ann_105 ;
                 schema:name "Graphia Report 2025" .

###  triple:ann_101
# Scenario A: External Match Found (Wikidata)
triple:ann_101 rdf:type owl:NamedIndividual ,
                        oa:Annotation ;
               oa:hasTarget triple:Document1 ;
               oa:hasBody <http://www.wikidata.org/entity/Q7259> ;
               oa:motivatedBy oa:identifying .

###  http://www.wikidata.org/entity/Q7259
# Enrichment of external URI
<http://www.wikidata.org/entity/Q7259> rdf:type owl:NamedIndividual ,
                                                schema:Person ;
                                       schema:name "Ada Lovelace" .


###  triple:ann_102
# Scenario B: Internal Match Found (Existing Entity)
triple:ann_102 rdf:type owl:NamedIndividual ,
                        oa:Annotation ;
               oa:hasTarget triple:Document1 ;
               oa:hasBody triple:JaneDoe ;
               oa:motivatedBy oa:identifying .

###  triple:JaneDoe
# Existing Internal Entity
triple:JaneDoe rdf:type owl:NamedIndividual ,
                        schema:Person ;
               schema:name "Jane Doe" .


###  triple:ann_103
# Scenario C: No Match / Ambiguous (New Local Entity)
triple:ann_103 rdf:type owl:NamedIndividual ,
                        oa:Annotation ;
               oa:hasTarget triple:Document1 ;
               oa:hasBody triple:person_1 ;
               oa:motivatedBy oa:identifying .

###  triple:person_1
# Definition of the new entity with Dual Typing
triple:person_1 rdf:type owl:NamedIndividual ,
                         schema:Person ,
                         skos:Concept ;
                schema:name "New Author Name" ;
                skos:prefLabel "New Author Name"@en .

###  triple:ann_104
# Scenario D: Place Entity
triple:ann_104 rdf:type owl:NamedIndividual ,
                        oa:Annotation ;
               oa:hasTarget triple:Document1 ;
               oa:hasBody triple:place_rome ;
               oa:motivatedBy oa:identifying .

###  triple:place_rome
triple:place_rome rdf:type owl:NamedIndividual ,
                           schema:Place ;
                  schema:name "Rome" .

###  triple:ann_105
# Scenario E: Concept Entity
triple:ann_105 rdf:type owl:NamedIndividual ,
                        oa:Annotation ;
               oa:hasTarget triple:Document1 ;
               oa:hasBody triple:concept_democracy ;
               oa:motivatedBy oa:identifying .

###  triple:concept_democracy
triple:concept_democracy rdf:type owl:NamedIndividual ,
                                  skos:Concept ;
                         schema:name "Democracy" ;
                         skos:prefLabel "Democracy"@en .
