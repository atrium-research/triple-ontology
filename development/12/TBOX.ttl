@prefix : <https://gotriple.eu/ontology/triple#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix schema: <http://schema.org/> .
@prefix triple: <https://gotriple.eu/ontology/triple#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix sioc: <http://rdfs.org/sioc/ns#> .
@prefix datacite: <http://purl.org/spar/datacite/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix litre: <http://purl.org/spar/literal/> .
@base <https://gotriple.eu/ontology/triple#> .

<https://gotriple.eu/ontology/triple> rdf:type owl:Ontology ;
                                       owl:versionIRI <https://gotriple.eu/ontology/triple/1.0.0> .

#################################################################
#    Annotation Properties
#################################################################


#################################################################
#    Object Properties
#################################################################

###  https://schema.org/spatialCoverage
schema:spatialCoverage rdf:type owl:ObjectProperty ;
                       rdfs:label "schema:spatialCoverage"@en ;
                       rdfs:comment "The spatialCoverage of a CreativeWork indicates the place(s) which are the focus of the content."@en ;
                       rdfs:range schema:Place .

###  https://schema.org/geo
schema:geo rdf:type owl:ObjectProperty ;
           rdfs:label "schema:geo"@en ;
           rdfs:comment "The geo coordinates of the place."@en ;
           rdfs:domain schema:Place ;
           rdfs:range schema:GeoShape .

###  https://schema.org/contactPoint
schema:contactPoint rdf:type owl:ObjectProperty ;
                    rdfs:label "schema:contactPoint"@en ;
                    rdfs:comment "A contact point for a person or organization."@en .

###  https://schema.org/author
schema:author rdf:type owl:ObjectProperty ;
              rdfs:label "schema:author"@en ;
              rdfs:comment "The author of this content or rating."@en .

###  https://schema.org/contributor
schema:contributor rdf:type owl:ObjectProperty ;
                   rdfs:label "schema:contributor"@en ;
                   rdfs:comment "A secondary contributor to the CreativeWork or Event."@en .

###  https://schema.org/publisher
schema:publisher rdf:type owl:ObjectProperty ;
                 rdfs:label "schema:publisher"@en ;
                 rdfs:comment "The publisher of the creative work."@en .

###  https://schema.org/funding
schema:funding rdf:type owl:ObjectProperty ;
               rdfs:label "schema:funding"@en ;
               rdfs:comment "A Grant that directly or indirectly provide funding or sponsorship for this item."@en .

###  https://schema.org/provider
schema:provider rdf:type owl:ObjectProperty ;
                rdfs:label "schema:provider"@en ;
                rdfs:comment "The service provider, service operator, or service performer; the goods producer."@en .


###  https://schema.org/mentions
schema:mentions rdf:type owl:ObjectProperty ;
                rdfs:label "schema:mentions"@en ;
                rdfs:comment "Indicates that the CreativeWork contains a reference to, but is not necessarily about a particular thing."@en .

###  http://rdfs.org/sioc/ns#topic
sioc:topic rdf:type owl:ObjectProperty ;
           rdfs:label "sioc:topic"@en ;
           rdfs:comment "A topic of interest, linking to the concept URI."@en .

###  http://purl.org/dc/terms/isReferencedBy
dcterms:isReferencedBy rdf:type owl:ObjectProperty ;
                       rdfs:label "dcterms:isReferencedBy"@en ;
                       rdfs:comment "A related resource that references, cites, or otherwise points to the described resource."@en .

###  https://schema.org/keywords
schema:keywords rdf:type owl:ObjectProperty ;
                rdfs:label "schema:keywords"@en ;
                rdfs:comment "Keywords or tags used to describe this content."@en ;
                rdfs:range schema:DefinedTerm .

###  http://purl.org/spar/datacite/hasIdentifier
datacite:hasIdentifier rdf:type owl:ObjectProperty ;
                       rdfs:label "datacite:hasIdentifier"@en ;
                       rdfs:comment "Links entities to their identifiers"@en ;
                       rdfs:range datacite:Identifier .

###  http://purl.org/spar/datacite/hasIdentifierScheme
datacite:hasIdentifierScheme rdf:type owl:ObjectProperty ;
                             rdfs:label "datacite:hasIdentifierScheme"@en ;
                             rdfs:comment "Links an identifier to its scheme"@en ;
                             rdfs:domain datacite:Identifier .

#################################################################
#    Data Properties
#################################################################

###  https://schema.org/name
schema:name rdf:type owl:DatatypeProperty ;
            rdfs:label "schema:name"@en ;
            rdfs:comment "The name of the item."@en ;
            rdfs:range xsd:string .

###  https://schema.org/temporalCoverage
schema:temporalCoverage rdf:type owl:DatatypeProperty ;
                        rdfs:label "schema:temporalCoverage"@en ;
                        rdfs:comment "The temporalCoverage of a CreativeWork indicates the period that the content applies to."@en .

###  https://schema.org/encodingFormat
schema:encodingFormat rdf:type owl:DatatypeProperty ;
                      rdfs:label "schema:encodingFormat"@en ;
                      rdfs:comment "Media type typically expressed using a MIME format."@en .

###  https://schema.org/size
schema:size rdf:type owl:DatatypeProperty ;
            rdfs:label "schema:size"@en ;
            rdfs:comment "A standardized size of a product or creative work."@en .


###  https://schema.org/creativeWorkStatus
schema:creativeWorkStatus rdf:type owl:DatatypeProperty ;
                          rdfs:label "schema:creativeWorkStatus"@en ;
                          rdfs:comment "The status of a creative work in terms of its stage in a lifecycle."@en .


###  https://schema.org/headline
schema:headline rdf:type owl:DatatypeProperty ;
                rdfs:label "schema:headline"@en ;
                rdfs:comment "Headline of the article."@en ;
                rdfs:range rdf:langString .

###  https://schema.org/abstract
schema:abstract rdf:type owl:DatatypeProperty ;
                rdfs:label "schema:abstract"@en ;
                rdfs:comment "An abstract is a short description that summarizes a CreativeWork."@en ;
                rdfs:range rdf:langString .

###  https://schema.org/additionalType
schema:additionalType rdf:type owl:DatatypeProperty ;
                      rdfs:label "schema:additionalType"@en ;
                      rdfs:comment "An additional type for the item, typically used for adding more specific types."@en .


###  https://schema.org/knowsAbout
schema:knowsAbout rdf:type owl:DatatypeProperty ;
                  rdfs:label "schema:knowsAbout"@en ;
                  rdfs:comment "Of a Person, and less typically of an Organization, to indicate a topic that is known about."@en .

###  https://schema.org/inLanguage
schema:inLanguage rdf:type owl:ObjectProperty ;
                  rdfs:label "schema:inLanguage"@en ;
                  rdfs:comment "The language of the content or performance or used in an action."@en .

###  https://schema.org/dateCreated
schema:dateCreated rdf:type owl:DatatypeProperty ;
                   rdfs:label "schema:dateCreated"@en ;
                   rdfs:comment "The date on which the CreativeWork was created or the item was added to a DataFeed."@en .

###  https://schema.org/datePublished
schema:datePublished rdf:type owl:DatatypeProperty ;
                     rdfs:label "schema:datePublished"@en ;
                     rdfs:comment "Date of first broadcast/publication."@en .

###  https://schema.org/dateModified
schema:dateModified rdf:type owl:DatatypeProperty ;
                    rdfs:label "schema:dateModified"@en ;
                    rdfs:comment "The date on which the CreativeWork was most recently modified or when the item's entry was modified within a DataFeed."@en .




###  https://schema.org/comment
schema:comment rdf:type owl:DatatypeProperty ;
               rdfs:label "schema:comment"@en ;
               rdfs:comment "Comments, typically from users."@en .

#################################################################
#    Classes
#################################################################

###  https://gotriple.eu/ontology/triple#Dataset
triple:Dataset rdf:type owl:Class ;
               rdfs:label "Dataset (Triple)"@en ;
               rdfs:comment "A dataset in the GoTriple platform, representing research data with specific metadata including spatial coverage, temporal coverage, and distribution information. It is a subclass of schema:Dataset."@en ;
               rdfs:subClassOf schema:Dataset ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:spatialCoverage ;
                               owl:allValuesFrom schema:Place ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:contactPoint ;
                               owl:allValuesFrom owl:Thing ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:author ;
                               owl:allValuesFrom [ owl:unionOf ( foaf:Person foaf:Organization ) ] ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:contributor ;
                               owl:allValuesFrom [ owl:unionOf ( foaf:Person foaf:Organization ) ] ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:publisher ;
                               owl:allValuesFrom [ owl:unionOf ( foaf:Person foaf:Organization ) ] ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:provider ;
                               owl:allValuesFrom [ owl:unionOf ( foaf:Person foaf:Organization ) ] ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:funding ;
                               owl:allValuesFrom owl:Thing ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:mentions ;
                               owl:allValuesFrom owl:Thing ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:keywords ;
                               owl:allValuesFrom schema:DefinedTerm ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty sioc:topic ;
                               owl:allValuesFrom owl:Thing ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty dcterms:isReferencedBy ;
                               owl:allValuesFrom owl:Thing ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:temporalCoverage ;
                               owl:allValuesFrom xsd:string ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:encodingFormat ;
                               owl:allValuesFrom xsd:string ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:size ;
                               owl:allValuesFrom xsd:string ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:creativeWorkStatus ;
                               owl:allValuesFrom xsd:string ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty datacite:hasIdentifier ;
                               owl:allValuesFrom datacite:Identifier ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:headline ;
                               owl:allValuesFrom rdf:langString ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:abstract ;
                               owl:allValuesFrom rdf:langString ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:additionalType ;
                               owl:allValuesFrom xsd:string ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:knowsAbout ;
                               owl:allValuesFrom xsd:string ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:inLanguage ;
                               owl:someValuesFrom schema:Language ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:dateCreated ;
                               owl:allValuesFrom xsd:date ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:datePublished ;
                               owl:allValuesFrom xsd:date ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:dateModified ;
                               owl:allValuesFrom xsd:date ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:dateCreated ;
                               owl:maxCardinality "1"^^xsd:nonNegativeInteger ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:datePublished ;
                               owl:maxCardinality "1"^^xsd:nonNegativeInteger ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:dateModified ;
                               owl:maxCardinality "1"^^xsd:nonNegativeInteger ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:comment ;
                               owl:allValuesFrom xsd:string ] .

###  https://schema.org/GeoShape  
schema:GeoShape rdf:type owl:Class ;
                rdfs:label "schema:GeoShape"@en ;
                rdfs:comment "The geographic shape of a place. A GeoShape can be described using several properties whose values are based on latitude/longitude pairs."@en .

###  https://schema.org/DefinedTerm
schema:DefinedTerm rdf:type owl:Class ;
                   rdfs:label "schema:DefinedTerm"@en ;
                   rdfs:comment "A word, name, acronym, phrase, etc. with a formal definition"@en .

###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class ;
            rdfs:label "foaf:Person"@en ;
            rdfs:comment "A person."@en .

###  http://xmlns.com/foaf/0.1/Organization
foaf:Organization rdf:type owl:Class ;
                  rdfs:label "foaf:Organization"@en ;
                  rdfs:comment "An organization."@en .

###  https://schema.org/Place
schema:Place rdf:type owl:Class ;
             rdfs:label "schema:Place"@en ;
             rdfs:comment "Entities that have a somewhat fixed, physical extension."@en ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty schema:name ;
                              owl:someValuesFrom xsd:string ] .

###  http://purl.org/spar/datacite/Identifier
datacite:Identifier rdf:type owl:Class ;
                   rdfs:label "datacite:Identifier"@en ;
                   rdfs:comment "An identifier that uniquely identifies an entity according to some identification scheme."@en .

###  http://purl.org/spar/datacite/IdentifierScheme
datacite:IdentifierScheme rdf:type owl:Class ;
                         rdfs:label "datacite:IdentifierScheme"@en ;
                         rdfs:comment "A scheme that uniquely identifies entities of a particular type."@en .


#################################################################
#    Individuals
#################################################################

###  https://gotriple.eu/ontology/triple#landing_page_url
triple:landing_page_url rdf:type owl:NamedIndividual ,
                                 datacite:IdentifierScheme ;
                        rdfs:label "Landing Page URL"@en ;
                        rdfs:comment "URL of the landing page containing metadata and descriptive information about the document."@en .


###  https://gotriple.eu/ontology/triple#full_text_url
triple:full_text_url rdf:type owl:NamedIndividual ,
                              datacite:IdentifierScheme ;
                     rdfs:label "Full Text URL"@en ;
                     rdfs:comment "URL providing direct access to the full content of the document."@en .


###  https://gotriple.eu/ontology/triple#source_url
triple:source_url rdf:type owl:NamedIndividual ,
                           datacite:IdentifierScheme ;
                  rdfs:label "Source URL"@en ;
                  rdfs:comment "URL of the original publication location or source repository."@en .


###  https://gotriple.eu/ontology/triple#internal_id
triple:internal_id rdf:type owl:NamedIndividual ,
                            datacite:IdentifierScheme ;
                   rdfs:label "Internal ID"@en ;
                   rdfs:comment "Internal identifier used within the GoTriple platform for document management."@en .


###  https://gotriple.eu/ontology/triple#pid
triple:pid rdf:type owl:NamedIndividual ,
                    datacite:IdentifierScheme ;
           rdfs:label "PID"@en ;
           rdfs:comment "Persistent identifier generated and exposed externally by the GoTriple platform."@en .


###  https://gotriple.eu/ontology/triple#original_id
triple:original_id rdf:type owl:NamedIndividual ,
                            datacite:IdentifierScheme ;
                   rdfs:label "Original ID"@en ;
                   rdfs:comment "Original identifier from the source system where the document was harvested."@en .


###  https://gotriple.eu/ontology/triple#doi
triple:doi rdf:type owl:NamedIndividual ,
                    datacite:IdentifierScheme ;
           rdfs:label "DOI"@en ;
           rdfs:comment "Digital Object Identifier - a persistent identifier for digital objects maintained by the International DOI Foundation."@en .


###  https://gotriple.eu/ontology/triple#handle
triple:handle rdf:type owl:NamedIndividual ,
                       datacite:IdentifierScheme ;
               rdfs:label "Handle"@en ;
               rdfs:comment "Handle System - a persistent identifier infrastructure for managing digital objects."@en .
