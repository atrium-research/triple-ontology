###  http://purl.org/spar/datacite/Identifier
datacite:Identifier rdf:type owl:Class ;
                    rdfs:label "datacite:Identifier"@en .


###  http://purl.org/spar/datacite/IdentifierScheme
datacite:IdentifierScheme rdf:type owl:Class ;
                          rdfs:label "datacite:IdentifierScheme"@en .


###  https://gotriple.eu/ontology/triple#ID
triple:ID rdf:type owl:Class ;
          rdfs:label "Internal ID"@en ;
          rdfs:comment "Internal identifier used within the GoTriple platform for resource management."@en ;
          rdfs:subClassOf datacite:Identifier ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty litre:hasLiteralValue ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty datacite:usesIdentifierScheme ;
                            owl:hasValue datacite:local-resource-identifier-scheme
                          ] .


###  https://gotriple.eu/ontology/triple#PID
triple:PID rdf:type owl:Class ;
           rdfs:label "Persistent ID"@en ;
           rdfs:comment "Persistent identifier generated and exposed externally by the GoTriple platform."@en ;
           rdfs:subClassOf datacite:Identifier ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty litre:hasLiteralValue ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty datacite:usesIdentifierScheme ;
                             owl:hasValue datacite:local-resource-identifier-scheme
                           ] .


###  https://gotriple.eu/ontology/triple#OriginalIdentifier
triple:OriginalIdentifier rdf:type owl:Class ;
                          rdfs:label "Original Identifier"@en ;
                          rdfs:comment "Original identifier from the source system where the resource was harvested."@en ;
                          rdfs:subClassOf datacite:Identifier ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty litre:hasLiteralValue ;
                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                            owl:onDataRange xsd:string
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty datacite:usesIdentifierScheme ;
                                            owl:hasValue datacite:local-resource-identifier-scheme
                                          ] .


###  http://schema.org/Language
<http://schema.org/Language> rdf:type owl:Class ;
                             rdfs:label "schema:Language"@en .


###  http://www.w3.org/2004/02/skos/core#Concept
<http://www.w3.org/2004/02/skos/core#Concept> rdf:type owl:Class ;
                                               rdfs:label "skos:Concept"@en .


###  http://schema.org/CreativeWork
schema:CreativeWork rdf:type owl:Class ;
                    rdfs:label "schema:CreativeWork"@en .


###  http://xmlns.com/foaf/0.1/Document
foaf:Document rdf:type owl:Class ;
              rdfs:label "foaf:Document"@en .


###  https://gotriple.eu/ontology/triple#Document
triple:Document rdf:type owl:Class ;
                rdfs:subClassOf schema:CreativeWork ,
                                foaf:Document ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty schema:additionalType ;
                                  owl:someValuesFrom skos:Concept
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty datacite:hasIdentifier ;
                                  owl:someValuesFrom triple:ID
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty datacite:hasIdentifier ;
                                  owl:someValuesFrom triple:PID
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty datacite:hasIdentifier ;
                                  owl:someValuesFrom triple:OriginalIdentifier
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://schema.org/inLanguage> ;
                                  owl:someValuesFrom <http://schema.org/Language>
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://schema.org/headline> ;
                                  owl:someValuesFrom rdf:langString
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://schema.org/abstract> ;
                                  owl:someValuesFrom rdf:langString
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://schema.org/encodingFormat> ;
                                  owl:someValuesFrom xsd:string
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty sioc:topic ;
                                  owl:someValuesFrom skos:Concept
                                ] ;
                rdfs:label "Document (Triple)"@en ;
                rdfs:comment "A document in the GoTriple platform, representing scholarly publications, research datasets, and other SSH research artifacts. It is a subclass of both schema:CreativeWork and foaf:Document."@en .


