###  Multimedia Example 3: Digital Art History Image Collection
triple:multimedia-003 rdf:type triple:MediaObject ;
                      datacite:hasIdentifier triple:identifier-multimedia-003 ;
                      schema:headline "High-Resolution Scan: Botticelli's Birth of Venus"@en ;
                      schema:abstract "Ultra-high resolution digital scan of Sandro Botticelli's The Birth of Venus (c. 1484-1486) from the Uffizi Gallery collection, created for digital humanities research and education."@en ;
                      schema:author "Uffizi Digital Archive Team" ;
                      schema:contributor "Dr. Francesca Marini", "Digital Humanities Lab" ;
                      schema:dateCreated "2023-07-12"^^xsd:date ;
                      schema:datePublished "2023-09-05"^^xsd:date ;
                      schema:publisher "Uffizi Gallery" ;
                      schema:provider "Europeana" ;
                      schema:license "CC BY-NC 4.0" ;
                      schema:keywords triple:keyword-renaissance-art, triple:keyword-botticelli, triple:keyword-digital-humanities, triple:keyword-cultural-heritage ;
                      schema:additionalType "image/tiff" ;
                      schema:knowsAbout "Renaissance painting", "Italian art", "digital preservation" ;
                      schema:inLanguage schema:language-en, schema:language-it ;
                      schema:spatialCoverage triple:place-florence-italy ;
                      schema:temporalCoverage "1480-1490" ;
                      schema:encodingFormat "image/tiff", "image/jpeg" ;
                      schema:size "850 MB" ;
                      datacite:hasIdentifier triple:identifier-image-media ,
                                             triple:identifier-image-landing ,
                                             triple:identifier-image-internal ,
                                             triple:identifier-image-pid ,
                                             triple:identifier-image-original ,
                                             triple:identifier-image-doi ,
                                             triple:identifier-image-handle ;
                      schema:conditionsOfAccess "Educational and Research Use" ;
                      schema:funding triple:project-digital-uffizi ;
                      sioc:topic triple:topic-art-history ;
                      dcterms:isReferencedBy triple:document-botticelli-analysis-2023 .

