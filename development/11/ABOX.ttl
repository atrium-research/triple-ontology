###  Multimedia Example 1: Canal-U Video Lecture
triple:multimedia-001 rdf:type triple:MediaObject ;
                      datacite:hasIdentifier triple:identifier-multimedia-001 ;
                      schema:headline "Introduction to Medieval History: The Carolingian Renaissance"@fr ;
                      schema:abstract "Conférence magistrale sur la Renaissance carolingienne présentée dans le cadre du séminaire d'histoire médiévale de l'Université de Paris."@fr ;
                      schema:author "Prof. Marie Dubois" ;
                      schema:contributor "Dr. Jean Martin", "Université de Paris" ;
                      schema:dateCreated "2023-10-15"^^xsd:date ;
                      schema:datePublished "2023-11-01"^^xsd:date ;
                      schema:dateModified "2023-11-02"^^xsd:date ;
                      schema:publisher "Canal-U" ;
                      schema:provider "Canal-U" ;
                      schema:license "CC BY-NC-ND 4.0" ;
                      schema:keywords triple:keyword-histoire-medievale, triple:keyword-renaissance-carolingienne, triple:keyword-charlemagne ;
                      schema:additionalType "video/mp4" ;
                      schema:knowsAbout "medieval history", "Carolingian period" ;
                      schema:inLanguage schema:language-fr ;
                      schema:spatialCoverage triple:place-europe-occidentale ;
                      schema:temporalCoverage "750-900 CE" ;
                      schema:duration "PT1H25M30S" ;
                      schema:encodingFormat "video/mp4" ;
                      schema:size "1.2 GB" ;
                      datacite:hasIdentifier triple:identifier-video-media ,
                                             triple:identifier-video-landing ,
                                             triple:identifier-video-internal ,
                                             triple:identifier-video-pid ,
                                             triple:identifier-video-original ,
                                             triple:identifier-video-doi ,
                                             triple:identifier-video-handle ;
                      schema:conditionsOfAccess "Open Access" ;
                      schema:funding triple:project-medieval-studies ;
                      sioc:topic triple:topic-medieval-history ;
                      schema:mentions triple:keyword-charlemagne, triple:keyword-alcuin ;
                      dcterms:isReferencedBy triple:document-medieval-anthology-2023 ;
                      schema:contactPoint triple:contact-canal-u .

triple:contact-canal-u rdf:type schema:ContactPoint ;
                       schema:email "contact@canal-u.fr" .

