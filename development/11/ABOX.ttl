###  Keywords (DefinedTerms)
triple:keyword-histoire-medievale rdf:type schema:DefinedTerm ;
                                  schema:name "histoire médiévale"@fr ;
                                  schema:description "L'étude académique de la période médiévale européenne"@fr .

triple:keyword-renaissance-carolingienne rdf:type schema:DefinedTerm ;
                                         schema:name "renaissance carolingienne"@fr ;
                                         schema:description "Période de renouveau culturel et intellectuel sous l'empire carolingien"@fr .

triple:keyword-charlemagne rdf:type schema:DefinedTerm ;
                           schema:name "Charlemagne"@fr ;
                           schema:description "Empereur carolingien et figure centrale du renouveau culturel médiéval"@fr .

triple:keyword-alcuin rdf:type schema:DefinedTerm ;
                      schema:name "Alcuin of York"@en ;
                      schema:description "English scholar, educator, and theologian who served as advisor to Charlemagne during the Carolingian Renaissance"@en .

triple:keyword-oral-history rdf:type schema:DefinedTerm ;
                            schema:name "oral history"@en ;
                            schema:description "Historical methodology using spoken testimonies and interviews"@en .

triple:keyword-world-war-ii rdf:type schema:DefinedTerm ;
                            schema:name "World War II"@en ;
                            schema:description "Global conflict from 1939 to 1945"@en .

triple:keyword-italian-resistance rdf:type schema:DefinedTerm ;
                                  schema:name "Italian Resistance"@en ;
                                  schema:description "Armed resistance movement against fascist occupation in Italy during WWII"@en .

triple:keyword-memory-studies rdf:type schema:DefinedTerm ;
                              schema:name "memory studies"@en ;
                              schema:description "Interdisciplinary field studying collective and cultural memory"@en .

triple:keyword-renaissance-art rdf:type schema:DefinedTerm ;
                               schema:name "Renaissance art"@en ;
                               schema:description "Artistic movement from 14th to 17th century Europe"@en .

triple:keyword-botticelli rdf:type schema:DefinedTerm ;
                          schema:name "Botticelli"@en ;
                          schema:description "Sandro Botticelli, Italian Renaissance painter (1445-1510)"@en .

triple:keyword-digital-humanities rdf:type schema:DefinedTerm ;
                                  schema:name "digital humanities"@en ;
                                  schema:description "Application of digital tools and methods to humanities research"@en .

triple:keyword-cultural-heritage rdf:type schema:DefinedTerm ;
                                 schema:name "cultural heritage"@en ;
                                 schema:description "Legacy of physical artifacts and cultural traditions inherited from past generations"@en .

