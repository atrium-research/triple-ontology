###  Semantic Artifacts Examples

###  Example 1: TRIPLE SSH Thesaurus
triple:thesaurus-ssh rdf:type triple:SemanticArtefact ;
                    schema:headline "TRIPLE SSH Thesaurus"@en ;
                    schema:abstract "Controlled vocabulary for Social Sciences and Humanities research classification used in the GoTriple discovery platform. Provides standardized terminology for SSH domains."@en ;
                    schema:contributor triple:organization-gotriple ;
                    schema:inLanguage schema:language-en ;
                    schema:datePublished "2023-03-15"^^xsd:date ;
                    schema:dateModified "2023-10-20"^^xsd:date ;
                    schema:version "2.1.0" ;
                    schema:publisher triple:organization-gotriple ;
                    triple:aggregator triple:organization-isidore ;
                    schema:fileFormat "application/rdf+xml" ;
                    schema:encodingFormat "SKOS vocabulary" ;
                    schema:contactPoint triple:contact-gotriple ;
                    schema:creativeWorkStatus triple:status-published ;
                    schema:keywords triple:keyword-thesaurus, triple:keyword-ssh, triple:keyword-vocabulary ;
                    datacite:hasIdentifier triple:identifier-thesaurus-doi ;
                    schema:identifier "gotriple:thesaurus:ssh:v2.1" ;
                    schema:url "https://thesaurus.gotriple.eu/download/rdf" ;
                    schema:mainEntityOfPage triple:page-thesaurus-landing ;
                    dcterms:isReferencedBy triple:document-dh-methodology .

###  Example 2: SKOS Art History Vocabulary
triple:vocab-arthistory rdf:type triple:SemanticArtefact ;
                       schema:headline "SKOS Art History Vocabulary"@en ;
                       schema:abstract "Comprehensive vocabulary for art historical concepts and terminology. Covers periods, movements, techniques, and cultural contexts in art history research."@en ;
                       schema:contributor triple:person-maria-rossi, triple:organization-dhi ;
                       schema:inLanguage schema:language-en ;
                       schema:datePublished "2023-05-10"^^xsd:date ;
                       schema:dateModified "2023-09-12"^^xsd:date ;
                       schema:version "1.3.2" ;
                       schema:publisher triple:organization-dhi ;
                       schema:fileFormat "text/turtle" ;
                       schema:encodingFormat "SKOS vocabulary" ;
                       schema:contactPoint triple:contact-dhi ;
                       schema:creativeWorkStatus triple:status-published ;
                       schema:keywords triple:keyword-arthistory, triple:keyword-vocabulary, triple:keyword-cultural-heritage ;
                       schema:identifier "dhi:vocab:arthistory:v1.3" ;
                       schema:url "https://vocab.arthistory.eu/skos.ttl" ;
                       schema:mainEntityOfPage triple:page-vocab-arthistory ;
                       schema:isBasedOnUrl "https://getty.edu/research/tools/vocabularies/aat/" .

###  Example 3: Medieval Studies Ontology
triple:ontology-medieval rdf:type triple:SemanticArtefact ;
                        schema:headline "Medieval Studies Ontology"@en ;
                        schema:abstract "Formal ontology for medieval studies research domain. Provides semantic modeling of medieval historical periods, events, places, and scholarly concepts."@en ;
                        schema:contributor triple:person-giovanni-bianchi, triple:person-pierre-martin ;
                        schema:inLanguage schema:language-en, schema:language-it ;
                        schema:datePublished "2023-01-20"^^xsd:date ;
                        schema:dateModified "2023-08-05"^^xsd:date ;
                        schema:version "0.9.1" ;
                        schema:publisher triple:organization-unibo ;
                        schema:fileFormat "application/owl+xml" ;
                        schema:encodingFormat "OWL ontology" ;
                        schema:contactPoint triple:contact-unibo-medieval ;
                        schema:creativeWorkStatus triple:status-draft ;
                        schema:keywords triple:keyword-medieval, triple:keyword-ontology, triple:keyword-history ;
                        datacite:hasIdentifier triple:identifier-medieval-handle ;
                        schema:identifier "unibo:ontology:medieval:v0.9" ;
                        schema:url "https://ontology.medieval.unibo.it/owl" ;
                        schema:mainEntityOfPage triple:page-medieval-landing ;
                        dcterms:isReferencedBy triple:document-carolingian .

