###  Status Values
triple:status-published rdf:type skos:Concept ;
                       skos:prefLabel "Published"@en ;
                       skos:definition "The semantic artifact has been officially published and is available for use"@en .

triple:status-draft rdf:type skos:Concept ;
                   skos:prefLabel "Draft"@en ;
                   skos:definition "The semantic artifact is in draft stage and under development"@en .

###  Keywords
triple:keyword-thesaurus rdf:type schema:DefinedTerm ;
                        schema:name "thesaurus" ;
                        schema:description "Controlled vocabulary with hierarchical relationships"@en .

triple:keyword-ssh rdf:type schema:DefinedTerm ;
                  schema:name "Social Sciences and Humanities" ;
                  schema:description "Academic disciplines studying human society and culture"@en .

triple:keyword-vocabulary rdf:type schema:DefinedTerm ;
                         schema:name "vocabulary" ;
                         schema:description "Set of terms used in a particular domain"@en .

triple:keyword-arthistory rdf:type schema:DefinedTerm ;
                         schema:name "art history" ;
                         schema:description "Academic study of art objects in their historical context"@en .

triple:keyword-cultural-heritage rdf:type schema:DefinedTerm ;
                                schema:name "cultural heritage" ;
                                schema:description "Legacy of physical artifacts and cultural traditions"@en .

triple:keyword-medieval rdf:type schema:DefinedTerm ;
                       schema:name "medieval studies" ;
                       schema:description "Academic field studying the Middle Ages period"@en .

triple:keyword-ontology rdf:type schema:DefinedTerm ;
                       schema:name "ontology" ;
                       schema:description "Formal specification of conceptualization in a domain"@en .

triple:keyword-history rdf:type schema:DefinedTerm ;
                      schema:name "history" ;
                      schema:description "Study of past events and human activities"@en .

