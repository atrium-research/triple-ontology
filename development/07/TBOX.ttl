@prefix : <https://gotriple.eu/ontology/triple#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix triple: <https://gotriple.eu/ontology/triple#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix schema: <http://schema.org/> .
@prefix datacite: <http://purl.org/spar/datacite/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@base <https://gotriple.eu/ontology/triple> .

<https://gotriple.eu/ontology/triple> rdf:type owl:Ontology ;
                                       owl:versionInfo "1.0.0" ;
                                       owl:imports <https://gotriple.eu/ontology/triple/project_types#> .

#################################################################
#    Object Properties
#################################################################

###  http://schema.org/about
schema:about rdf:type owl:ObjectProperty ;
             rdfs:label "schema:about"@en ;
             rdfs:comment "The subject matter of the content"@en .


###  http://schema.org/funder
schema:funder rdf:type owl:ObjectProperty ;
              rdfs:label "schema:funder"@en ;
              rdfs:comment "A person or organization that supports something through some kind of financial contribution"@en .


###  http://schema.org/funding
schema:funding rdf:type owl:ObjectProperty ;
               rdfs:label "schema:funding"@en ;
               rdfs:comment "A grant, typically financial or otherwise quantifiable, of resources"@en .


###  http://schema.org/keywords
schema:keywords rdf:type owl:ObjectProperty ;
                rdfs:label "schema:keywords"@en ;
                rdfs:comment "Keywords or tags used to describe this content"@en .


###  http://schema.org/sponsor
schema:sponsor rdf:type owl:ObjectProperty ;
               rdfs:label "schema:sponsor"@en ;
               rdfs:comment "A person or organization that supports a thing through a pledge, promise, or financial contribution"@en .


###  http://schema.org/organizer
schema:organizer rdf:type owl:ObjectProperty ;
                 rdfs:label "schema:organizer"@en ;
                 rdfs:comment "The organization who organizes the event or project."@en ;
                 rdfs:range schema:Organization .


###  http://schema.org/knowsAbout
schema:knowsAbout rdf:type owl:ObjectProperty ;
                  rdfs:label "schema:knowsAbout"@en ;
                  rdfs:comment "Of a Person, and less typically of an Organization, to indicate a topic that is known about - suggested for areas of expertise."@en ;
                  rdfs:range skos:Concept .





###  http://schema.org/mainEntityOfPage
schema:mainEntityOfPage rdf:type owl:DatatypeProperty ;
                        rdfs:label "schema:mainEntityOfPage"@en ;
                        rdfs:comment "Indicates a page (or other CreativeWork) for which this thing is the main entity being described."@en ;
                        rdfs:range schema:URL .

###  https://gotriple.eu/ontology/triple#hasProjectType
triple:hasProjectType rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf dcterms:type ;
                      rdfs:range triple:ProjectType ;
                      rdfs:label "has project type"@en .


#################################################################
#    Data properties
#################################################################

###  http://schema.org/dateCreated
schema:dateCreated rdf:type owl:DatatypeProperty ;
                   rdfs:label "schema:dateCreated"@en ;
                   rdfs:comment "The date on which the CreativeWork was created or the item was added to a DataFeed."@en ;
                   rdfs:range xsd:date .

###  http://schema.org/dateModified
schema:dateModified rdf:type owl:DatatypeProperty ;
                    rdfs:label "schema:dateModified"@en ;
                    rdfs:comment "The date on which the CreativeWork was most recently modified or when the item's entry was modified within a DataFeed."@en ;
                    rdfs:range xsd:date .

###  http://schema.org/alternateName
schema:alternateName rdf:type owl:DatatypeProperty ;
                     rdfs:label "schema:alternateName"@en ;
                     rdfs:comment "An alias for the item"@en ;
                     rdfs:range rdf:langString .


###  http://schema.org/description
schema:description rdf:type owl:DatatypeProperty ;
                   rdfs:label "schema:description"@en ;
                   rdfs:comment "A description of the item"@en ;
                   rdfs:range rdf:langString .


###  http://schema.org/endDate
schema:endDate rdf:type owl:DatatypeProperty ;
               rdfs:label "schema:endDate"@en ;
               rdfs:comment "The end date and time of the item"@en ;
               rdfs:range xsd:date .


###  http://schema.org/startDate
schema:startDate rdf:type owl:DatatypeProperty ;
                 rdfs:label "schema:startDate"@en ;
                 rdfs:comment "The start date and time of the item"@en ;
                 rdfs:range xsd:date .





#################################################################
#    Classes
#################################################################


###  http://schema.org/Language
schema:Language rdf:type owl:Class ;
                rdfs:label "schema:Language"@en ;
                rdfs:comment "Natural language such as Spanish, Tamil, or English."@en .

###  http://schema.org/DefinedTerm
schema:DefinedTerm rdf:type owl:Class ;
                   rdfs:label "schema:DefinedTerm"@en ;
                   rdfs:comment "A word, name, acronym, phrase, etc. with a formal definition"@en .



###  http://schema.org/URL
schema:URL rdf:type rdfs:Datatype ;
           rdfs:label "schema:URL"@en ;
           rdfs:comment "Data type: URL."@en .

###  https://gotriple.eu/ontology/triple#ProjectType
triple:ProjectType rdf:type owl:Class ;
                   rdfs:subClassOf skos:Concept ;
                   rdfs:label "Project Type"@en .


###  http://schema.org/Grant
schema:Grant rdf:type owl:Class ;
             rdfs:label "schema:Grant"@en ;
             rdfs:comment "A grant, typically financial or otherwise quantifiable, of resources"@en ;

             # Optional funder (0 or more)
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:funder ;
                               owl:allValuesFrom [ owl:unionOf (foaf:Person foaf:Organization) ]
                             ] ;

             # Optional sponsor (0 or more)
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty schema:sponsor ;
                               owl:allValuesFrom [ owl:unionOf (foaf:Person foaf:Organization) ]
                             ] .


###  http://schema.org/Project
triple:Project rdf:type owl:Class ;
               rdfs:label "Project (Triple)"@en ;
               rdfs:comment "A research project in the Social Sciences and Humanities domain integrated into the GoTriple platform. It is a subclass of schema:Project."@en ;
               rdfs:subClassOf schema:Project ;

               # Required name (1 or more, multilingual)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:name ;
                                 owl:minCardinality "1"^^xsd:nonNegativeInteger
                               ] ;

               # Optional identifier (0 or more)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty datacite:hasIdentifier ;
                                 owl:allValuesFrom [ rdf:type owl:Class ;
                                                     owl:unionOf ( datacite:Identifier triple:ID triple:PID triple:OriginalIdentifier ) ]
                               ] ;

               # Optional start date (0 or 1)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:startDate ;
                                 owl:maxCardinality "1"^^xsd:nonNegativeInteger
                               ] ;

               # Date type restrictions
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:startDate ;
                                 owl:allValuesFrom xsd:date
                               ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:endDate ;
                                 owl:allValuesFrom xsd:date
                               ] ;
               
               # Optional end date (0 or 1)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:endDate ;
                                 owl:maxCardinality "1"^^xsd:nonNegativeInteger
                               ] ;

               # Optional funding (0 or more)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:funding ;
                                 owl:allValuesFrom schema:Grant
                               ] ;

               # Optional topic/discipline (0 or more)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:about ;
                                 owl:allValuesFrom skos:Concept
                               ] ;

               # Optional keywords (0 or more)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:keywords ;
                                 owl:allValuesFrom schema:DefinedTerm
                               ] ;

               # Optional description (0 or more, multilingual)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:description ;
                                 owl:allValuesFrom rdf:langString
                               ] ;

               # Optional alternate name/acronym (0 or more, multilingual)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:alternateName ;
                                 owl:allValuesFrom rdf:langString
                               ] ;

               # Optional organizer (0 or more)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:organizer ;
                                 owl:allValuesFrom schema:Organization
                               ] ;

               # Optional knowsAbout (0 or more)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:knowsAbout ;
                                 owl:allValuesFrom skos:Concept
                               ] ;

               # Optional mainEntityOfPage (0 or more)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:mainEntityOfPage ;
                                 owl:allValuesFrom schema:URL
                               ] ;

               # Optional language (0 or more)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:inLanguage ;
                                 owl:someValuesFrom schema:Language
                               ] ;

               # Optional date created (0 or 1)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:dateCreated ;
                                 owl:maxCardinality "1"^^xsd:nonNegativeInteger
                               ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:dateCreated ;
                                 owl:allValuesFrom xsd:date
                               ] ;

               # Optional date modified (0 or 1)
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:dateModified ;
                                 owl:maxCardinality "1"^^xsd:nonNegativeInteger
                               ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:dateModified ;
                                 owl:allValuesFrom xsd:date
                               ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty schema:contactPoint ;
                                 owl:allValuesFrom schema:ContactPoint
                               ] ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty triple:hasProjectType ;
                                 owl:allValuesFrom triple:ProjectType
                               ] .






