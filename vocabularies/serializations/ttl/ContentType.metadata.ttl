@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .

<https://gotriple.eu/ontology/triple/ContentType> a owl:Ontology ;
    dcterms:title "Content Type Vocabulary"@en ;
    dcterms:description "The Content Type vocabulary categorizes the different types of resources available in the TRIPLE platform, such as journal articles, datasets, images, and video. It includes mappings to the COAR Resource Type Vocabulary to ensure interoperability."@en ;
    dcterms:abstract "A hierarchical controlled vocabulary for classifying the nature and genre of scholarly resources."@en ;
    vann:preferredNamespacePrefix "contenttype" ;
    vann:preferredNamespaceUri "https://gotriple.eu/ontology/triple/ContentType/" .
